<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="NWWL0020Query">
    
    <typeAlias alias="NWWL0020SMsg"    type="business.blap.NWWL0020.NWWL0020SMsg"/>
    <typeAlias alias="NWWL0020_ASMsg"  type="business.blap.NWWL0020.NWWL0020_ASMsg"/>

    <statement id="getNtfyActionList" parameterClass="Map" resultMap="result.getNtfyActionList">
        SELECT
             DTL.NTFY_ACT_DTL_PK
            ,DTL.NTFY_ACT_ID
            ,DTL.NTFY_ACT_NM
            ,DTL.NTFY_ACT_DESC_TXT
            ,DTL.EZUPTIME
            ,DTL.EZUPTIMEZONE
            ,TP.NTFY_ACT_TP_DESC_TXT
        FROM
             NTFY_ACT_DTL  DTL
            ,NTFY_ACT_TP   TP
        WHERE
                DTL.GLBL_CMPY_CD    = #glblCmpyCd#
            AND DTL.NTFY_HDR_PK     = #ntfyHdrPk#
            AND DTL.EZCANCELFLAG    = '0'
        
            AND DTL.GLBL_CMPY_CD    = TP.GLBL_CMPY_CD
            AND DTL.NTFY_ACT_TP_CD  = TP.NTFY_ACT_TP_CD
            AND DTL.EZCANCELFLAG    = '0'
        ORDER BY
            DTL.NTFY_ACT_ID  ASC
    </statement>
    <resultMap id="result.getNtfyActionList"   class="NWWL0020_ASMsg">
        <result property="ntfyActDtlPk_A0"      column="NTFY_ACT_DTL_PK"       javaType="EZDSStringItem"/>
        <result property="ntfyActId_A0"         column="NTFY_ACT_ID"           javaType="EZDSStringItem"/>
        <result property="ntfyActNm_A0"         column="NTFY_ACT_NM"           javaType="EZDSStringItem"/>
        <result property="ntfyActDescTxt_A0"    column="NTFY_ACT_DESC_TXT"     javaType="EZDSStringItem"/>
        <result property="ezUpTime_A0"          column="EZUPTIME"              javaType="EZDSStringItem"/>
        <result property="ezUpTimeZone_A0"      column="EZUPTIMEZONE"          javaType="EZDSStringItem"/>
        <result property="ntfyActTpDescTxt_A0"  column="NTFY_ACT_TP_DESC_TXT"  javaType="EZDSStringItem"/>
    </resultMap>

    <statement id="execSql" parameterClass="String" resultClass="Map">
        $sqlTxt$
    </statement>

</sqlMap>
