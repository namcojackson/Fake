<sqlMap namespace="NFCB127001Query">

    <typeAlias alias="AR_ACCT_DTTMsg_alias" type="business.db.AR_ACCT_DTTMsg"/>
    <statement id="getArAcctDt" parameterClass="Map" resultMap="result.AR_ACCT_DTTMsg">
     SELECT
               A.ACCT_YR_MTH
     FROM
               AR_ACCT_DT  A
     WHERE
                    A.GLBL_CMPY_CD   =  #GLBL_CMPY_CD#
               AND  A.SUB_SYS_CD     =  #SUB_SYS_CD#
               AND  A.ONL_BAT_TP_CD  =  #ONL_BAT_TP_CD#
               AND  A.EZCANCELFLAG   =  '0'
    </statement>
    <resultMap id="result.AR_ACCT_DTTMsg" class="AR_ACCT_DTTMsg_alias">
        <result property="acctYrMth" column="ACCT_YR_MTH" javaType="EZDTDateItem"/>
    </resultMap>

    <typeAlias alias="AR_ADJTMsg_alias" type="business.db.AR_ADJTMsg"/>
    <statement id="getArAdj" parameterClass="Map" resultMap="result.AR_ADJTMsg">
     SELECT
               SUM(DEAL_AR_ADJ_AMT)  DEAL_AR_ADJ_AMT
              ,SUM(FUNC_AR_ADJ_AMT)  FUNC_AR_ADJ_AMT
     FROM
               AR_ADJ  A
     WHERE
                    A.GLBL_CMPY_CD        =  #GLBL_CMPY_CD#
               AND  A.GL_DT            &gt;  #GL_DT_THRU#
               AND  A.ORIG_AR_TRX_BAL_PK  =  #ORIG_AR_TRX_BAL_PK#
               AND  A.AR_ADJ_STS_CD       =  #AR_ADJ_STS_CD#
               AND  A.EZCANCELFLAG        =  '0'
    </statement>
    <resultMap id="result.AR_ADJTMsg" class="AR_ADJTMsg_alias">
        <result property="dealArAdjAmt" column="DEAL_AR_ADJ_AMT" javaType="EZDTBigDecimalItem"/>
        <result property="funcArAdjAmt" column="FUNC_AR_ADJ_AMT" javaType="EZDTBigDecimalItem"/>
    </resultMap>

</sqlMap>
