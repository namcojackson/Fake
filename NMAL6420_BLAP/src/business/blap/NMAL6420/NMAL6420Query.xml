<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NMAL6420Query">

	<typeAlias alias="NMAL6420CMsg"		type="business.blap.NMAL6420.NMAL6420CMsg"/>
	<typeAlias alias="NMAL6420_ASMsg"	type="business.blap.NMAL6420.NMAL6420_ASMsg"/>

	<resultMap id="result.getNMAL6420"    class="NMAL6420_ASMsg">
		<result property="glblCmpyCd_A1"         column="GLBL_CMPY_CD"           javaType="EZDCStringItem"/>
		<result property="locRoleTpSortNum_A1"   column="LOC_ROLE_TP_SORT_NUM"   javaType="EZDCBigDecimalItem"/>
		<result property="locRoleTpCd_A1"        column="LOC_ROLE_TP_CD"         javaType="EZDCStringItem"/>
		<result property="locRoleTpNm_A1"        column="LOC_ROLE_TP_NM"         javaType="EZDCStringItem"/>
		<result property="locRoleTpDescTxt_A1"   column="LOC_ROLE_TP_DESC_TXT"   javaType="EZDCStringItem"/>
		<result property="acctCdFlg_A1"          column="ACCT_CD_FLG"            javaType="EZDCStringItem"/>
		<result property="locGrpCd_A1"           column="LOC_GRP_CD"             javaType="EZDCStringItem"/>
		<result property="ezUpTime_A1"           column="EZUPTIME"               javaType="EZDCStringItem"/>
		<result property="ezUpTimeZone_A1"       column="EZUPTIMEZONE"           javaType="EZDCStringItem"/>
	</resultMap>
	
    <statement id="getCodeTable" parameterClass="NMAL6420CMsg" resultMap="result.getNMAL6420">
        SELECT 
            LRT.GLBL_CMPY_CD           GLBL_CMPY_CD
           ,LRT.LOC_ROLE_TP_CD         LOC_ROLE_TP_CD
           ,LRT.LOC_ROLE_TP_NM         LOC_ROLE_TP_NM
           ,LRT.LOC_ROLE_TP_SORT_NUM   LOC_ROLE_TP_SORT_NUM
           ,LRT.LOC_ROLE_TP_DESC_TXT   LOC_ROLE_TP_DESC_TXT
           ,LRT.ACCT_CD_FLG            ACCT_CD_FLG
           ,LRT.LOC_GRP_CD             LOC_GRP_CD
           ,NVL(LRT.EZUPTIME, '*')     EZUPTIME
		   ,NVL(LRT.EZUPTIMEZONE, '*') EZUPTIMEZONE
        FROM 
            LOC_ROLE_TP    LRT
        WHERE
                 LRT.GLBL_CMPY_CD = #glblCmpyCd#
            AND  LRT.EZCANCELFLAG = '0'
        ORDER BY
            LRT.LOC_ROLE_TP_SORT_NUM
    </statement>
	
</sqlMap>
