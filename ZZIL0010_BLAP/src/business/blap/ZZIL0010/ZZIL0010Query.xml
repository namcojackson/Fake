<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="ZZIL0010Query">

	<typeAlias alias="ZZIL0010_ASMsg"	type="business.blap.ZZIL0010.ZZIL0010_ASMsg"/>
	
	<statement id="getTransList" parameterClass="Map" resultMap="result.ZZIL0010_ASMsg">
		SELECT
		        T.TRANSACTION_ID            AS  TRANSACTION_ID
		    ,   T.EZINTIME					AS	EZINTIME
		    ,   T.EZUPTIME					AS	EZUPTIME
		    ,	T.EZUPTIMEZONE				AS	EZUPTIMEZONE
		    ,   T.PROCESSED_REC_CNT			AS	PROCESSED_REC_CNT
		    ,   T.FAILED_REC_CNT			AS	FAILED_REC_CNT
		    ,	T.PROCESSED_FLAG			AS	PROCESSED_FLAG
		FROM
		        INTERFACE_TRANSACTION T
		WHERE
				T.INTERFACE_ID  = #cMsg.interfaceId#
			AND T.EZCANCELFLAG  = '0'
				
		<!-- transactionId -->
		<isNotNull property="cMsg.transactionId">
			AND T.TRANSACTION_ID = #cMsg.transactionId#
		</isNotNull>
		
		<!-- processedFlag -->
		<isNotNull property="cMsg.processedFlag_PS">
			AND T.PROCESSED_FLAG = #cMsg.processedFlag_PS#
		</isNotNull>
		
		<!-- registerDateFrom -->
		<isNotNull property="registerDateFrom">
			AND T.EZINTIME &gt;= #registerDateFrom#
		</isNotNull>
			
		<!-- registerDateTo -->
		<isNotNull property="registerDateTo">
			AND T.EZINTIME &lt;= #registerDateTo#
		</isNotNull>

		<!-- updatedDateFrom -->
		<isNotNull property="updatedDateFrom">
			AND T.EZUPTIME &gt;= #updatedDateFrom#
		</isNotNull>
			
		<!-- updatedDateTo -->
		<isNotNull property="updatedDateTo">
			AND T.EZUPTIME &lt;= #updatedDateTo#
		</isNotNull>
			
		ORDER BY
		        T.TRANSACTION_ID
	</statement>
	
	<resultMap id="result.ZZIL0010_ASMsg" 		class="ZZIL0010_ASMsg">
		<result property="transactionId_A"		column="TRANSACTION_ID"			javaType="EZDSBigDecimalItem"/>
		<result property="ezInTime_A"			column="EZINTIME"				javaType="EZDSStringItem"/>
		<result property="ezUpTime_A"			column="EZUPTIME"				javaType="EZDSStringItem"/>
		<result property="ezUpTimeZone_A"		column="EZUPTIMEZONE"			javaType="EZDSStringItem"/>
		<result property="processedRecCnt_A"	column="PROCESSED_REC_CNT"		javaType="EZDSBigDecimalItem"/>
		<result property="failedRecCnt_A"		column="FAILED_REC_CNT"			javaType="EZDSBigDecimalItem"/>
		<result property="processedFlag_A"		column="PROCESSED_FLAG"			javaType="EZDSStringItem"/>
	</resultMap>

</sqlMap>