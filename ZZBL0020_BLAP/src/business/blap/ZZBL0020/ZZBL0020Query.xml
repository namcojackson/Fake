<?xml version="1.0" encoding="UTF-8"?>
<sqlMap namespace="ZZBL0020Query">
	
	<typeAlias alias="ZZBL0020_ASMsg" type="business.blap.ZZBL0020.ZZBL0020_ASMsg"/>
    <typeAlias alias="ZZBL0020_ASMsgArray" type="business.blap.ZZBL0020.ZZBL0020_ASMsgArray"/>
	
	<statement id="getReqMasterList" resultMap="result.ZZBL0020_ASMsg" parameterClass="Map">
		SELECT 
			   '1' as ACTV_FLG
			  ,EZREQBUSINESSID
			  ,EZREQBUSINESSNAME
			  ,EZREQJOBCTLNETID
			  ,EZREQJOBCONCURRENCY
			  ,EZREQJOBSTOPFLAG
			  ,EZREQJOBERRORCTLFLAG
			  ,EZREQCOUNTPERJOB
              ,EZUPTIME
              ,EZUPTIMEZONE
	    FROM ARM10 
		WHERE 
			EZCANCELFLAG = 0 
			<isNotNull property="ezReqBusinessID">
				AND UPPER(EZREQBUSINESSID) like UPPER(#ezReqBusinessID#)
			</isNotNull>
			<isNotNull property="ezReqJobCtlNetID">
				AND UPPER(EZREQJOBCTLNETID) like UPPER(#ezReqJobCtlNetID#)
			</isNotNull>
			Order by EZREQBUSINESSID
	</statement>
	
	<resultMap id="result.ZZBL0020_ASMsg" class="ZZBL0020_ASMsg">
		<result property="actvFlg_A" column="ACTV_FLG" javaType="StringItemEZD"/>	
		<result property="ezReqBusinessID_A" column="EZREQBUSINESSID" javaType="StringItemEZD"/>
		<result property="ezReqBusinessName_A" column="EZREQBUSINESSNAME" javaType="StringItemEZD"/>	
		<result property="ezReqJobCtlNetID_A" column="EZREQJOBCTLNETID" javaType="StringItemEZD"/>	
		<result property="ezReqJobConcurrency_A" column="EZREQJOBCONCURRENCY" javaType="StringItemEZD"/>	
		<result property="ezReqJobStopFlag_A" column="EZREQJOBSTOPFLAG" javaType="StringItemEZD"/>							
		<result property="ezReqJobErrorCtlFlag_A" column="EZREQJOBERRORCTLFLAG" javaType="StringItemEZD"/>
		<result property="ezReqCountPerJob_A" column="EZREQCOUNTPERJOB" javaType="EZDSBigDecimalItem"/>
		<result property="ezUpTime" column="EZUPTIME" javaType="StringItemEZD"/>		
		<result property="ezUpTimeZone" column="EZUPTIMEZONE" javaType="StringItemEZD"/>		
	</resultMap>
</sqlMap>    

