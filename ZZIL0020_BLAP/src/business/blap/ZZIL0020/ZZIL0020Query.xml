<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="ZZIL0020Query">

	<typeAlias alias="INTERFACE_SETTINGTMsg"		type="business.blap.ZZIL0020.ZZIL0020_ASMsg"/>
	<typeAlias alias="INTERFACE_SENDERTMsg"			type="business.blap.ZZIL0020.ZZIL0020_BSMsg"/>
	<typeAlias alias="INTERFACE_RECEIVERTMsg"		type="business.blap.ZZIL0020.ZZIL0020_CSMsg"/>
	<typeAlias alias="INTERFACE_MQCONFIGTMsg"		type="business.blap.ZZIL0020.ZZIL0020_DSMsg"/>
	<typeAlias alias="INTERFACE_MQCONFIG_JNDITMsg"	type="business.blap.ZZIL0020.ZZIL0020_ESMsg"/>
	
	<!-- Download INTERFACE_SETTING -->
	<statement id="INTERFACE_SETTING" resultMap="result.INTERFACE_SETTINGTMsg">
		SELECT
				INTERFACE_ID,
				DRCTN_TP,
				COMM_TOOL,
				INTFC_SEND_ID,
				INTFC_RCV_ID,
				MQ_CONFIG_ID,
				MQ_CONFIG_JNDI_ID,
				INTFC_DESC
		FROM
		        INTERFACE_SETTING
		WHERE
				EZCANCELFLAG  = '0'
		ORDER BY
		        INTERFACE_ID
	</statement>

	<!-- Download INTERFACE_SENDER -->
	<statement id="INTERFACE_SENDER" resultMap="result.INTERFACE_SENDERTMsg">
		SELECT
				INTFC_SEND_ID,
				MSG_SEND_CLS,
				MSG_TIMEOUT
		FROM
		        INTERFACE_SENDER
		WHERE
				EZCANCELFLAG  = '0'
		ORDER BY
		        INTFC_SEND_ID
	</statement>

	<!-- Download INTERFACE_RECEIVER -->
	<statement id="INTERFACE_RECEIVER" resultMap="result.INTERFACE_RECEIVERTMsg">
		SELECT
				INTFC_RCV_ID,
				EZBUSINESSID,
				EZFUNCTIONCODE,
				EZUSERID,
				EZPASSWORD,
				SEC_CONFIG_NM
		FROM
		        INTERFACE_RECEIVER
		WHERE
				EZCANCELFLAG  = '0'
		ORDER BY
		        INTFC_RCV_ID
	</statement>

	<!-- Download INTERFACE_MQCONFIG -->
	<statement id="INTERFACE_MQCONFIG" resultMap="result.INTERFACE_MQCONFIGTMsg">
		SELECT
				MQ_CONFIG_ID,
				TRNSP_TP,
				HOST_NM,
				CH_NM,
				PORT_NUM,
				QUEUE_CONNECT_USR,
				QUEUE_CONNECT_PASSWORD,
				QUEUE_MGR,
				QUEUE_NM,
				REPLY_QUEUE_NM,
				FAIL_QUEUE_NM
		FROM
		        INTERFACE_MQCONFIG
		WHERE
				EZCANCELFLAG  = '0'
		ORDER BY
		        MQ_CONFIG_ID
	</statement>

	<!-- Download INTERFACE_MQCONFIG_JNDI -->
	<statement id="INTERFACE_MQCONFIG_JNDI" resultMap="result.INTERFACE_MQCONFIG_JNDITMsg">
		SELECT
				MQ_CONFIG_JNDI_ID,
				PROVIDER_URL,
				QUEUE_FACTORY_JNDI_NM,
				QUEUE_JNDI_NM,
				REPLY_QUEUE_JNDI_NM,
				FAIL_QUEUE_JNDI_NM
		FROM
		        INTERFACE_MQCONFIG_JNDI
		WHERE
				EZCANCELFLAG  = '0'
		ORDER BY
		        MQ_CONFIG_JNDI_ID
	</statement>

	<!-- Existence check INTFC_SEND_ID -->
	<statement id="INTFC_SEND_ID" parameterClass="Map" resultClass="Map">
		SELECT
				INTFC_SEND_ID
		FROM
				INTERFACE_SENDER
		WHERE
				EZCANCELFLAG = '0'
			AND INTFC_SEND_ID = #intfcId#
	</statement>

	<!-- Existence check INTFC_RCV_ID -->
	<statement id="INTFC_RCV_ID" parameterClass="Map" resultClass="Map">
		SELECT
				INTFC_RCV_ID
		FROM
				INTERFACE_RECEIVER
		WHERE
				EZCANCELFLAG = '0'
			AND INTFC_RCV_ID = #intfcId#
	</statement>

	<!-- Existence check MQ_CONFIG_ID -->
	<statement id="MQ_CONFIG_ID" parameterClass="Map" resultClass="Map">
		SELECT
				MQ_CONFIG_ID
		FROM
				INTERFACE_MQCONFIG
		WHERE
				EZCANCELFLAG = '0'
			AND MQ_CONFIG_ID = #intfcId#
	</statement>

	<!-- Existence check MQ_CONFIG_JNDI_ID -->
	<statement id="MQ_CONFIG_JNDI_ID" parameterClass="Map" resultClass="Map">
		SELECT
				MQ_CONFIG_JNDI_ID
		FROM
				INTERFACE_MQCONFIG_JNDI
		WHERE
				EZCANCELFLAG = '0'
			AND MQ_CONFIG_JNDI_ID = #intfcId#
	</statement>
	
	<resultMap id="result.INTERFACE_SETTINGTMsg" 	class="INTERFACE_SETTINGTMsg">
		<result property="interfaceId_A"		column="INTERFACE_ID"			javaType="EZDSStringItem"/>
		<result property="drctnTp_A"			column="DRCTN_TP"				javaType="EZDSStringItem"/>
		<result property="commTool_A"			column="COMM_TOOL"				javaType="EZDSStringItem"/>
		<result property="intfcSendId_A"		column="INTFC_SEND_ID"			javaType="EZDSStringItem"/>
		<result property="intfcRcvId_A"			column="INTFC_RCV_ID"			javaType="EZDSStringItem"/>
		<result property="mqConfigId_A"			column="MQ_CONFIG_ID"			javaType="EZDSStringItem"/>
		<result property="mqConfigJndiId_A"		column="MQ_CONFIG_JNDI_ID"		javaType="EZDSStringItem"/>
		<result property="intfcDesc_A"			column="INTFC_DESC"				javaType="EZDSStringItem"/>
	</resultMap>

	<resultMap id="result.INTERFACE_SENDERTMsg" 	class="INTERFACE_SENDERTMsg">
		<result property="intfcSendId_B"		column="INTFC_SEND_ID"			javaType="EZDSStringItem"/>
		<result property="msgSendCls_B"			column="MSG_SEND_CLS"			javaType="EZDSStringItem"/>
		<result property="msgTimeout_B"			column="MSG_TIMEOUT"			javaType="EZDSBigDecimalItem"/>
	</resultMap>

	<resultMap id="result.INTERFACE_RECEIVERTMsg" 	class="INTERFACE_RECEIVERTMsg">
		<result property="intfcRcvId_C"			column="INTFC_RCV_ID"			javaType="EZDSStringItem"/>
		<result property="ezBusinessID_C"		column="EZBUSINESSID"			javaType="EZDSStringItem"/>
		<result property="ezFunctionCode_C"		column="EZFUNCTIONCODE"			javaType="EZDSStringItem"/>
		<result property="ezUserID_C"			column="EZUSERID"				javaType="EZDSStringItem"/>
		<result property="ezPassword_C"			column="EZPASSWORD"				javaType="EZDSStringItem"/>
		<result property="secConfigNm_C"		column="SEC_CONFIG_NM"			javaType="EZDSStringItem"/>
	</resultMap>

	<resultMap id="result.INTERFACE_MQCONFIGTMsg" 	class="INTERFACE_MQCONFIGTMsg">
		<result property="mqConfigId_D"				column="MQ_CONFIG_ID"			javaType="EZDSStringItem"/>
		<result property="trnspTp_D"				column="TRNSP_TP"				javaType="EZDSStringItem"/>
		<result property="hostNm_D"					column="HOST_NM"				javaType="EZDSStringItem"/>
		<result property="chNm_D"					column="CH_NM"					javaType="EZDSStringItem"/>
		<result property="portNum_D"				column="PORT_NUM"				javaType="EZDSStringItem"/>
		<result property="queueConnectUsr_D"		column="QUEUE_CONNECT_USR"		javaType="EZDSStringItem"/>
		<result property="queueConnectPassword_D"	column="QUEUE_CONNECT_PASSWORD"	javaType="EZDSStringItem"/>
		<result property="queueMgr_D"				column="QUEUE_MGR"				javaType="EZDSStringItem"/>
		<result property="queueNm_D"				column="QUEUE_NM"				javaType="EZDSStringItem"/>
		<result property="replyQueueNm_D"			column="REPLY_QUEUE_NM"			javaType="EZDSStringItem"/>
		<result property="failQueueNm_D"			column="FAIL_QUEUE_NM"			javaType="EZDSStringItem"/>
	</resultMap>

	<resultMap id="result.INTERFACE_MQCONFIG_JNDITMsg" 	class="INTERFACE_MQCONFIG_JNDITMsg">
		<result property="mqConfigJndiId_E"		column="MQ_CONFIG_JNDI_ID"		javaType="EZDSStringItem"/>
		<result property="providerUrl_E"		column="PROVIDER_URL"			javaType="EZDSStringItem"/>
		<result property="queueFactoryJndiNm_E"	column="QUEUE_FACTORY_JNDI_NM"	javaType="EZDSStringItem"/>
		<result property="queueJndiNm_E"		column="QUEUE_JNDI_NM"			javaType="EZDSStringItem"/>
		<result property="replyQueueJndiNm_E"	column="REPLY_QUEUE_JNDI_NM"	javaType="EZDSStringItem"/>
		<result property="failQueueJndiNm_E"	column="FAIL_QUEUE_JNDI_NM"		javaType="EZDSStringItem"/>
	</resultMap>

</sqlMap>