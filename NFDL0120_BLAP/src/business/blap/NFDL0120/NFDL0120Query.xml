<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NFDL0120Query">
    <typeAlias alias="NFDL0120_ACMsg" type="business.blap.NFDL0120.NFDL0120_ACMsg"/>
    <typeAlias alias="NFDL0120_NCMsg" type="business.blap.NFDL0120.NFDL0120_NCMsg"/>

    <statement id="getPulldownList" parameterClass="Map" resultMap="result.getPulldownList">
        SELECT
             CLT_WRK_ITEM_CD
            ,CLT_WRK_ITEM_NM
        FROM
            (
                SELECT
                     CWI.CLT_WRK_ITEM_CD
                    ,CWI.CLT_WRK_ITEM_NM
                FROM
                    CLT_WRK_ITEM CWI
                WHERE
                        CWI.GLBL_CMPY_CD = #glblCmpyCd#
                    AND CWI.EZCANCELFLAG = '0'
                ORDER BY
                     CWI.CLT_WRK_ITEM_CD
            )
        WHERE
            ROWNUM &lt;= #rowNum#
    </statement>

    <statement id="getSearchData" parameterClass="Map" resultMap="result.getSearchData">
        SELECT
             CLT_WRK_ITEM_CD
            ,CLT_WRK_ITEM_NM
            ,CLT_WRK_ITEM_SORT_NUM
            ,EZUPTIME
            ,EZUPTIMEZONE
            ,XX_TP_CD
        FROM
            (
                SELECT
                     CSRWI.CLT_WRK_ITEM_CD
                    ,CWI.CLT_WRK_ITEM_NM
                    ,CSRWI.CLT_WRK_ITEM_SORT_NUM
                    ,CSRWI.EZUPTIME
                    ,CSRWI.EZUPTIMEZONE
                    ,#xxTpCd_Upd#             AS XX_TP_CD
                FROM
                     CLT_STRGY_RELN_WRK_ITEM CSRWI
                    ,CLT_WRK_ITEM CWI
                WHERE
                        CSRWI.GLBL_CMPY_CD    = #glblCmpyCd#
                    AND CSRWI.CLT_STRGY_CD    = #cltStrgyCd#
                    AND CSRWI.GLBL_CMPY_CD    = CWI.GLBL_CMPY_CD
                    AND CSRWI.CLT_WRK_ITEM_CD = CWI.CLT_WRK_ITEM_CD
                    AND CSRWI.EZCANCELFLAG    = '0'
                    AND CWI.EZCANCELFLAG      = '0'
                ORDER BY
                    CSRWI.CLT_WRK_ITEM_SORT_NUM
            )
        WHERE
            ROWNUM &lt;= #rowNum#
    </statement>

    <resultMap id="result.getPulldownList" class="NFDL0120_NCMsg">
        <result property="cltWrkItemCd_N"    column="CLT_WRK_ITEM_CD"    javaType="EZDSStringItem"/>
        <result property="cltWrkItemNm_N"    column="CLT_WRK_ITEM_NM"    javaType="EZDSStringItem"/>
    </resultMap>

    <resultMap id="result.getSearchData" class="NFDL0120_ACMsg">
        <result property="cltWrkItemCd_SV"       column="CLT_WRK_ITEM_CD"         javaType="EZDSStringItem"/>
        <result property="cltWrkItemNm_A"        column="CLT_WRK_ITEM_NM"         javaType="EZDSStringItem"/>
        <result property="cltWrkItemSortNum_A"   column="CLT_WRK_ITEM_SORT_NUM"   javaType="EZDSBigDecimalItem"/>
        <result property="ezUpTime_A"            column="EZUPTIME"                javaType="EZDSStringItem"/>
        <result property="ezUpTimeZone_A"        column="EZUPTIMEZONE"            javaType="EZDSStringItem"/>
        <result property="xxTpCd_A"              column="XX_TP_CD"                javaType="EZDSStringItem"/>
    </resultMap>
</sqlMap>
