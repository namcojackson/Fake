<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="NSBB049001">

    <statement id="getDsBizLastProcTs" parameterClass="Map" resultClass="Map">
        SELECT
              DS_BIZ_LAST_PROC_TS
             ,DS_BIZ_PROC_LOG_PK

        FROM
             DS_BIZ_PROC_LOG
        WHERE
             GLBL_CMPY_CD                             =   #glblCmpyCd#
             AND PROC_PGM_ID                          =   #procPgmId#
             AND EZCANCELFLAG                         =   '0'
        ORDER BY
            DS_BIZ_LAST_PROC_TS DESC
    </statement>

    <statement id="getSvcMblDictInfo" parameterClass="Map" resultClass="Map">
        SELECT * FROM SVC_MBL_DICT
        WHERE
             GLBL_CMPY_CD                             =   #glblCmpyCd#
             AND EZCANCELFLAG                         =   '0'
    </statement>

    <statement id="getSendDictionaryInfo" parameterClass="Map" resultClass="Map">
          $gqlString$
    </statement>

</sqlMap>
