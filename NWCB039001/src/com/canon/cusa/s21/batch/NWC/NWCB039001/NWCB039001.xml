<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="NWCB039001">

    <statement id="getTblColCnt" parameterClass="Map" resultClass="Integer">
        SELECT
            COUNT(*)
        FROM
            ALL_TAB_COLUMNS UT
        WHERE
            UT.TABLE_NAME = #tableName#
    </statement>
    
</sqlMap>