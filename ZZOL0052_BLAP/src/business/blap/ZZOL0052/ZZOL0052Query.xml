<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="ZZOL0052Query">

	<typeAlias alias="ZZOL0052_ACMsg"	type="business.blap.ZZOL0052.ZZOL0052_ACMsg"/>

	<statement id="getList" parameterClass="Map" resultMap="result.ZZOL0052_ACMsg">
		SELECT
				row_number() over(order by MENU_PROC_ID)	AS	ROW_NUM
			,	MENU_PROC_ID
			,	MENU_PROC_NM
		FROM
		        MENU_PROC
		WHERE
				EZCANCELFLAG  = '0'
			AND GLBL_CMPY_CD  = #glblCmpyCd#
			<!-- Process ID -->
		<isNotNull property="upldCsvRstProcNm">
			AND MENU_PROC_NM LIKE #upldCsvRstProcNm#
		</isNotNull>
	
	</statement>

	<resultMap id="result.ZZOL0052_ACMsg" 		class="ZZOL0052_ACMsg">
		<result property="xxNum_A"				column="ROW_NUM"					javaType="EZDCBigDecimalItem"/>
		<result property="menuProcId_A"			column="MENU_PROC_ID"				javaType="EZDCStringItem"/>
		<result property="menuProcNm_A"			column="MENU_PROC_NM"				javaType="EZDCStringItem"/>
	</resultMap>
	
</sqlMap>