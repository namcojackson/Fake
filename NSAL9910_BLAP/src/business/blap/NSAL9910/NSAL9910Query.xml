<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NSAL9910Query">
    <typeAlias alias="NSAL9910_ACMsg"  type="business.blap.NSAL9910.NSAL9910_ACMsg"/>

    <statement id="getTrgtTbl" parameterClass="Map" resultMap="result.getTrgtTbl">
            SELECT
                 PHYS_MAINT_TRGT_TBL_NM
                ,LOGIC_MAINT_TRGT_TBL_NM
            FROM (
                SELECT
                     A.PHYS_MAINT_TRGT_TBL_NM   AS  PHYS_MAINT_TRGT_TBL_NM
                    ,A.LOGIC_MAINT_TRGT_TBL_NM  AS  LOGIC_MAINT_TRGT_TBL_NM
                FROM
                    MAINT_TRGT_TBL_INFO         A
                WHERE
                        A.GLBL_CMPY_CD          = #glblCmpyCd#
            <!-- START 2016/05/16 [QC#8147, DEL] -->
            <!--    AND A.BIZ_FUNC_GRP_ID       = #bizFuncGrpId# -->
            <!-- END 2016/05/16 [QC#8147, DEL] -->
            <!-- START 2018/06/15 [QC#24320, MOD] -->
            <!-- START 2018/06/21 K.Kojima [QC#24320,MOD] -->
            <!-- <isEqual property="referenceUserFlg" compareValue="false"> -->
            <!--         <iterate property="bizFuncIdList" var="bizFuncId[]" open="AND A.BIZ_FUNC_ID IN (" close=")" conjunction=","> -->
            <!--         #bizFuncId[]# -->
            <!--         </iterate> -->
            <!-- </isEqual> -->
            <iterate property="bizFuncIdList" var="bizFuncId[]" open="AND A.BIZ_FUNC_ID IN (" close=")" conjunction=",">
            #bizFuncId[]#
            </iterate>
            <!-- END 2018/06/21 K.Kojima [QC#24320,MOD] -->
            <!-- END 2018/06/15 [QC#24320, MOD] -->
                    AND A.EZCANCELFLAG          = '0'
                ORDER BY
                    A.LOGIC_MAINT_TRGT_TBL_NM
            )
            WHERE
                ROWNUM        &lt;= #rowNum#
    </statement>

    <resultMap id="result.getTrgtTbl"             class="NSAL9910_ACMsg">
        <result property="physMaintTrgtTblNm_A"   column="PHYS_MAINT_TRGT_TBL_NM"   javaType="EZDSStringItem" />
        <result property="logicMaintTrgtTblNm_A"  column="LOGIC_MAINT_TRGT_TBL_NM"  javaType="EZDSStringItem" />
    </resultMap>

</sqlMap>
