<?xml version="1.0" encoding="UTF-8"?>
<sqlMap namespace="NYEL0050Query">

	<typeAlias alias="NYEL0050_ACMsg" type="business.blap.NYEL0050.NYEL0050_ACMsg"/>
    <typeAlias alias="NYEL0050_ACMsgArray" type="business.blap.NYEL0050.NYEL0050_ACMsgArray"/>

	<statement id="getBusinessApps" resultMap="result.NYEL0050_ACMsg" parameterClass="Map">
		SELECT
			GLBL_CMPY_CD,
			BIZ_APP_ID,
			BIZ_APP_NM,
			ACTV_FLG,
			APP_DESC_TXT
	    FROM
	    	CD_MENU_TBL 
	    	WHERE
	    	<isNotNull property="glblCmpyCd">
				GLBL_CMPY_CD = #glblCmpyCd#
			</isNotNull>
			AND ACTV_FLG = 'Y'
			AND EZCANCELFLAG     = '0'
			ORDER BY GLBL_CMPY_CD DESC
	</statement>

	<resultMap id="result.NYEL0050_ACMsg" class="NYEL0050_ACMsg">
		<result property="glblCmpyCd" column="GLBL_CMPY_CD" javaType="StringItemEZD"/>
		<result property="bizAppId" column="BIZ_APP_ID" javaType="StringItemEZD"/>
		<result property="bizAppNm" column="BIZ_APP_NM" javaType="StringItemEZD"/>
		<result property="actvFlg" column="ACTV_FLG" javaType="StringItemEZD"/>
		<result property="appDescTxt" column="APP_DESC_TXT" javaType="StringItemEZD"/>
	</resultMap>
		
</sqlMap>
