<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NFDL0130Query">
    <typeAlias alias="NFDL0130_ACMsg" type="business.blap.NFDL0130.NFDL0130_ACMsg"/>
    <typeAlias alias="NFDL0130_NCMsg" type="business.blap.NFDL0130.NFDL0130_NCMsg"/>

    <statement id="getPulldownList" parameterClass="Map" resultMap="result.getPulldownList">
        SELECT
             CLT_WRK_TP_CD
            ,CLT_WRK_TP_DESC_TXT
        FROM
            (
                SELECT
                     CWT.CLT_WRK_TP_CD
                    ,CWT.CLT_WRK_TP_DESC_TXT
                FROM
                    CLT_WRK_TP CWT
                WHERE
                        CWT.GLBL_CMPY_CD = #glblCmpyCd#
                    AND CWT.EZCANCELFLAG = '0'
                ORDER BY
                    CWT.CLT_WRK_TP_SORT_NUM
            )
        WHERE
            ROWNUM &lt;= #rowNum#
    </statement>

    <statement id="getCltStrgyRelnWrkItem" parameterClass="Map" resultMap="String">
        SELECT
            CSRWI.GLBL_CMPY_CD
        FROM
            CLT_STRGY_RELN_WRK_ITEM CSRWI
        WHERE
                CSRWI.GLBL_CMPY_CD    = #glblCmpyCd#
            AND CSRWI.CLT_WRK_ITEM_CD = #cltWrkItemCd#
            AND CSRWI.EZCANCELFLAG    = '0'
            AND ROWNUM                = 1
    </statement>

    <resultMap id="result.getPulldownList" class="NFDL0130_NCMsg">
        <result property="cltWrkTpCd_N"    column="CLT_WRK_TP_CD"          javaType="EZDSStringItem"/>
        <result property="cltWrkTpNm_N"    column="CLT_WRK_TP_DESC_TXT"    javaType="EZDSStringItem"/>
    </resultMap>
</sqlMap>
