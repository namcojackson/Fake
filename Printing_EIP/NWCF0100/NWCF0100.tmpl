		AND H.GLBL_CMPY_CD = S.GLBL_CMPY_CD
		AND H.INV_BILL_NUM = S.INV_NUM
		AND S.EZCANCELFLAG = M.EZCANCELFLAG
		AND S.GLBL_CMPY_CD = M.GLBL_CMPY_CD
		AND S.INV_PRT_MAINT_SUB_TOT_PK = M.INV_PRT_MAINT_SUB_TOT_PK
		AND M.EZCANCELFLAG = MTR.EZCANCELFLAG(+)
		AND M.GLBL_CMPY_CD = MTR.GLBL_CMPY_CD(+)
		AND M.INV_PRT_MAINT_MACH_PK = MTR.INV_PRT_MAINT_MACH_PK(+)
		AND MTR.EZCANCELFLAG = X.EZCANCELFLAG(+)
		AND MTR.GLBL_CMPY_CD = X.GLBL_CMPY_CD(+)
		AND MTR.INV_PRT_MAINT_MTR_PK = X.INV_PRT_MAINT_MTR_PK(+)
		AND H.INV_BILL_NUM = ${INV_KEY}
        AND H.INV_PRT_CTRL_PK = ${P_INV_PRT_CTRL_PK}
