		   INV_AVG_GRP_NM
		   ,INV_AVG_GRP_NM_SP
		   ,INV_AVG_GRP_INFO_TXT
		   ,INV_AVG_GRP_INFO_TXT_SP
		   ,LOC_INFO_KEY
		   ,CONTR_KEY
		   ,DTL_PK
		   ,METER_KEY ASC NULLS FIRST
		   ,XS_METER_KEY
	"/>

	<variable name="sql6_mtrCnt" type="sql" value="
	SELECT
		COUNT(*) AS METER_COUNT
	FROM
	(
		SELECT DISTINCT
			   MTR.MTR_LB_NM AS MTR_LB_NM
			FROM 
			    INV_PRT_HDR H
			   ,INV_PRT_MAINT_SUB_TOT S
			   ,INV_PRT_MAINT_MACH M
			   ,INV_PRT_MAINT_MTR MTR
			   ,INV_PRT_MAINT_XS_MTR X
			WHERE 
			    H.EZCANCELFLAG = '0'
			AND H.GLBL_CMPY_CD = ${P_GLBL_CMPY_CD}
			AND H.EZCANCELFLAG = S.EZCANCELFLAG
			AND H.GLBL_CMPY_CD = S.GLBL_CMPY_CD
			AND H.INV_BILL_NUM = S.INV_NUM
			AND S.EZCANCELFLAG = M.EZCANCELFLAG
			AND S.GLBL_CMPY_CD = M.GLBL_CMPY_CD
			AND S.INV_PRT_MAINT_SUB_TOT_PK = M.INV_PRT_MAINT_SUB_TOT_PK
			AND M.EZCANCELFLAG = MTR.EZCANCELFLAG(+)
			AND M.GLBL_CMPY_CD = MTR.GLBL_CMPY_CD(+)
			AND M.INV_PRT_MAINT_MACH_PK = MTR.INV_PRT_MAINT_MACH_PK(+)
			AND MTR.EZCANCELFLAG = X.EZCANCELFLAG(+)
			AND MTR.GLBL_CMPY_CD = X.GLBL_CMPY_CD(+)
			AND MTR.INV_PRT_MAINT_MTR_PK = X.INV_PRT_MAINT_MTR_PK(+)
			AND MTR.MTR_LB_NM IS NOT NULL
			AND H.INV_BILL_NUM = ${INV_KEY}
	        AND H.INV_PRT_CTRL_PK = ${P_INV_PRT_CTRL_PK}
	)
	"/>
	
	<variable name="sql6_rllovr" type="sql" value="
	SELECT
		COUNT(*) AS NUM_RESULTS
	FROM
	(
		SELECT DISTINCT
			DCBM.BLLG_ROLL_OVER_RATIO
			, DCBM.BLLG_FREE_COPY_CNT
			, DCBM.CUM_COPY_CNT
		FROM
			INV_PRT_MAINT_MTR IPMTR,
			INV_PRT_MAINT_MACH IPMACH,
			DS_CONTR_DTL DCD,
			DS_CONTR_BLLG_MTR DCBM,
			INV_PRT_MAINT_CHRG_DTL IPCD,
			DS_CONTR_BLLG_SCHD DCBS
		WHERE
			IPMTR.EZCANCELFLAG = '0'
		AND IPMTR.GLBL_CMPY_CD = ${P_GLBL_CMPY_CD}
		AND IPMTR.GLBL_CMPY_CD = IPMACH.GLBL_CMPY_CD
		AND IPMTR.INV_PRT_MAINT_MACH_PK = IPMACH.INV_PRT_MAINT_MACH_PK
		AND IPMTR.INV_NUM = ${P_INV_BILL_NUM}
		AND IPMTR.INV_NUM = IPMACH.INV_NUM
		AND IPMACH.EZCANCELFLAG = '0'
		AND IPMACH.GLBL_CMPY_CD = DCD.GLBL_CMPY_CD
		AND IPMACH.DS_CONTR_PK = DCD.DS_CONTR_PK
		AND DCD.EZCANCELFLAG = '0'
		AND DCD.GLBL_CMPY_CD = DCBM.GLBL_CMPY_CD
		AND DCD.DS_CONTR_DTL_PK = DCBM.DS_CONTR_DTL_PK
		AND DCBM.EZCANCELFLAG = '0'
		AND IPCD.EZCANCELFLAG = '0'
		AND IPCD.GLBL_CMPY_CD = IPMACH.GLBL_CMPY_CD
		AND IPCD.INV_PRT_MAINT_MACH_PK = IPMACH.INV_PRT_MAINT_MACH_PK
		AND IPCD.INV_NUM = IPMTR.INV_NUM
		AND IPCD.LINE_TOT_AMT_TXT IS NULL
		AND DCBS.GLBL_CMPY_CD = IPCD.GLBL_CMPY_CD
		AND DCBS.EZCANCELFLAG = '0'
		AND IPMTR.INV_NUM = DCBS.SVC_INV_NUM
		AND DCBS.DS_CONTR_BLLG_MTR_PK = DCBM.DS_CONTR_BLLG_MTR_PK
		AND ((DCBM.BLLG_ROLL_OVER_RATIO IS NOT NULL AND DCBM.BLLG_ROLL_OVER_RATIO &lt;&gt; 0)
			OR (DCBM.BLLG_FREE_COPY_CNT IS NOT NULL AND DCBM.CUM_COPY_CNT IS NULL)
			OR (DCBM.CUM_COPY_CNT IS NOT NULL
				AND DCBM.CUM_COPY_START_DT &lt;= DCBS.BLLG_SCHD_THRU_DT
				AND DCBM.CUM_COPY_END_DT &gt;= DCBS.BLLG_SCHD_THRU_DT
			)
		)
	)
	"/>
	
	<!-- 6.Maintenance (Consolidated) -->
	<variable name="sql6_Consl" type="sql" value="
        WITH TOTAL_AMT_TBL AS (
			SELECT 
				S.BILL_MAINT_SUB_TOT_SQ
			   ,CASE WHEN SUM(S.INV_ST_TAX_AMT) &lt; 0 THEN 
					'(' || to_char(ABS(SUM(S.INV_ST_TAX_AMT)), 'FM999,999,999,999,990.00$') || ')' 
				ELSE 
					to_char(SUM(S.INV_ST_TAX_AMT), 'FM999,999,999,999,990.00$') 
				END ST_TAX_AMT
			   ,CASE WHEN SUM(S.INV_CNTY_TAX_AMT) &lt; 0 THEN 
					'(' || to_char(ABS(SUM(S.INV_CNTY_TAX_AMT)), 'FM999,999,999,999,990.00$') || ')' 
				ELSE 
					to_char(SUM(S.INV_CNTY_TAX_AMT), 'FM999,999,999,999,990.00$') 
				END CNTY_TAX_AMT
			   ,CASE WHEN SUM(S.INV_CITY_TAX_AMT) &lt; 0 THEN 
					'(' || to_char(ABS(SUM(S.INV_CITY_TAX_AMT)), 'FM999,999,999,999,990.00$') || ')' 
				ELSE 
					to_char(SUM(S.INV_CITY_TAX_AMT), 'FM999,999,999,999,990.00$') 
				END CITY_TAX_AMT
			   ,CASE WHEN SUM(S.INV_TOT_TAX_AMT) &lt; 0 THEN 
					'(' || to_char(ABS(SUM(S.INV_TOT_TAX_AMT)), 'FM999,999,999,999,990.00$') || ')' 
				ELSE 
					to_char(SUM(S.INV_TOT_TAX_AMT), 'FM999,999,999,999,990.00$') 
				END TOT_TAXES_AMT
			   ,CASE WHEN SUM(S.INV_SUB_TOT_AMT) &lt; 0 THEN 
					'(' || to_char(ABS(SUM(S.INV_SUB_TOT_AMT)), 'FM999,999,999,999,990.00$') || ')' 
				ELSE 
					to_char(SUM(S.INV_SUB_TOT_AMT), 'FM999,999,999,999,990.00$') 
				END  INV_SUB_TOT_AMT
			FROM 
			    INV_PRT_HDR H
			   ,INV_PRT_MAINT_SUB_TOT S
,INV_PRT_HDR H_CHLD
			WHERE 
			    H.EZCANCELFLAG = '0'
			AND H.GLBL_CMPY_CD = ${P_GLBL_CMPY_CD}
			AND H.EZCANCELFLAG = S.EZCANCELFLAG
			AND H.GLBL_CMPY_CD = S.GLBL_CMPY_CD
			AND H.INV_BILL_NUM = S.CONSL_BILL_NUM
			AND H.INV_BILL_NUM = ${INV_KEY}
	        AND H.INV_PRT_CTRL_PK = ${P_INV_PRT_CTRL_PK}
AND S.GLBL_CMPY_CD = H_CHLD.GLBL_CMPY_CD
AND S.EZCANCELFLAG = H_CHLD.EZCANCELFLAG
AND S.INV_NUM = H_CHLD.INV_BILL_NUM
AND (H_CHLD.EXCL_BILL_TXT = 'NO' OR H_CHLD.EXCL_BILL_TXT IS NULL)
			GROUP BY 
				S.BILL_MAINT_SUB_TOT_SQ
        )
		SELECT 
            'sql6_Consl' 																			sqlid
		   ,S.INV_AVG_GRP_NM 																INV_AVG_GRP_NM
		   ,CASE WHEN S.INV_AVG_GRP_NM IS NULL THEN NULL ELSE ' ' END 						INV_AVG_GRP_NM_SP
		   ,S.INV_AVG_GRP_INFO_TXT 															INV_AVG_GRP_INFO_TXT
		   ,CASE WHEN S.INV_AVG_GRP_INFO_TXT IS NULL THEN NULL ELSE ' ' END 				INV_AVG_GRP_INFO_TXT_SP
		   ,S.BILL_MAINT_SUB_TOT_SQ 														LOC_INFO_KEY
		   ,M.INV_PRT_MAINT_MACH_PK 														CONTR_KEY
		   ,D.INV_PRT_MAINT_CHRG_DTL_PK 													DTL_PK
		   ,NULL 																			METER_KEY
		   ,NULL 																			XS_METER_KEY
		   ,regexp_replace(
		    S.SHIP_TO_CUST_ACCT_CD || CHR(13) || CHR(10) ||
		    S.SHIP_TO_CUST_ACCT_NM || CHR(13) || CHR(10) ||
		    S.SHIP_TO_FIRST_LINE_ADDR || CHR(13) || CHR(10) ||
		    S.SHIP_TO_SCD_LINE_PRNT_ADDR || CHR(13) || CHR(10) || 
			S.SHIP_TO_CTY_ADDR || ', ' || S.SHIP_TO_ST_CD || ' ' || S.SHIP_TO_POST_CD
		    , '('|| CHR (13) || CHR (10) || '){2,}', (CHR (13) || CHR (10))) 				LOC_INFO
		   ,S.CUST_ISS_PO_NUM 																CUST_ISS_PO_NUM
		   ,regexp_replace(
		    S.FIRST_BLLG_ATTRB_VAL_TXT || CHR (13) || CHR (10) || 
		    S.SCD_BLLG_ATTRB_VAL_TXT || CHR (13) || CHR (10) || 
		    S.THIRD_BLLG_ATTRB_VAL_TXT || CHR (13) || CHR (10) || 
		    S.FRTH_BLLG_ATTRB_VAL_TXT || CHR (13) || CHR (10) || 
		    S.FIFTH_BLLG_ATTRB_VAL_TXT || CHR (13) || CHR (10) || 
		    S.SIXTH_BLLG_ATTRB_VAL_TXT
		    , '('|| CHR (13) || CHR (10) || ')+$', '') 										CUST_CODE
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN ${C22666} || ':' ELSE NULL END) 		LB_TAXES
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN ${C4483} || ':' ELSE NULL END) 		LB_STATE
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN TAT.ST_TAX_AMT ELSE NULL END) 	        ST_TAX_AMT
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN ${C1075} || ':' ELSE NULL END) 		LB_COUNTY
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN TAT.CNTY_TAX_AMT ELSE NULL END) 	    CNTY_TAX_AMT
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN ${C855} || ':' ELSE NULL END) 			LB_CITY
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN TAT.CITY_TAX_AMT ELSE NULL END) 	    CITY_TAX_AMT
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN ${C6604} || ':' ELSE NULL END) 		LB_TOTALTAXAMOUNT
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN TAT.TOT_TAXES_AMT ELSE NULL END)     	TOT_TAXES_AMT
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'Y' THEN ' ' ELSE NULL END) 					LB_SP6
		   ,NULL 																			LB_CHARGES
		   ,NULL 																			INV_TOT_CHRG_AMT
		   ,NULL 																			LB_SP7
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'N' THEN ${C6604} || ':' ELSE NULL END) 		LB_TOTALTAX
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'N' THEN TAT.TOT_TAXES_AMT ELSE NULL END) 	    TOT_TAX_AMT
		   ,(CASE WHEN H.DPLY_TAX_DTL_FLG = 'N' THEN ' ' ELSE NULL END) 					LB_SP8
		   ,' ' 																			LB_SP9_LINE
		   ,${C22667} || ':' 																LB_SUBTOTALS
		   ,TAT.INV_SUB_TOT_AMT 															INV_SUB_TOT_AMT
		   ,' ' 																			LB_SP10
		   ,M.DS_CONTR_NUM 																	DS_CONTR_NUM
		   ,M.INV_PRT_SER_NUM_TXT 															INV_PRT_SER_NUM_TXT
		   ,M.MDL_NM 																		MDL_NM
		   ,M.SVC_PGM_NM 																	SVC_PGM_NM
		   ,D.INV_NUM 																		INV_NUM
		   ,CASE WHEN D.INV_CHRG_TP_NM LIKE '%Usage%' THEN '1' ELSE '0' END                 INV_CHRG_TP_CD 
		   ,D.INV_CHRG_TP_NM 																INV_CHRG_TP_NM
		   ,D.BLLG_PER_FROM_DT_TXT || ' - ' || D.BLLG_PER_THRU_DT_TXT 						BLLG_PER_DT_TXT
		   ,D.LINE_TOT_AMT_TXT 																LINE_TOT_AMT_TXT
		   ,NULL 																			MTR_LB_NM
		   ,NULL 																			PREV_TOT_COPY_QTY_TXT
		   ,NULL 																			TOT_COPY_QTY_TXT
		   ,NULL 																			TEST_COPY_QTY_TXT
		   ,NULL 																			BLLG_COPY_QTY_TXT
		   ,NULL 																			CONTR_MTR_MULT_RATE_TXT
		   ,NULL 																			MLY_COPY_INCL_PRC_QTY_TXT
		   ,NULL																			ROLL_OVER_USED_QTY_DPLY_TXT
		   ,NULL																			NEXT_ROLL_OVER_QTY_DPLY_TXT
		   ,NULL																			ROLL_OVER_QTY_DPLY_TXT
		   ,NULL 																			MTR_COPY_QTY_TXT
		   ,NULL 																			XS_MTR_AMT_RATE_TXT
		   ,NULL 																			LINE_MTR_TOT_AMT_TXT
		   ,NULL 																			CONTR_XS_MTR_TXT
		   ,NULL 																			CONTR_XS_MTR_TXT_SP
           ,P.PMT_CC_FLG                                                                    PMT_CC_FLG
		FROM 
		    INV_PRT_HDR H
		   ,INV_PRT_MAINT_SUB_TOT S
		   ,INV_PRT_MAINT_MACH M
		   ,INV_PRT_MAINT_CHRG_DTL D
		   ,TOTAL_AMT_TBL TAT
           ,INV I
           ,PMT_TERM_CASH_DISC P
,INV_PRT_HDR H_CHLD
