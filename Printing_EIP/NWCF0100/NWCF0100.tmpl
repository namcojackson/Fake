	<variable name="sql9_rllovr" type="sql" value="
	SELECT
		COUNT(*) AS NUM_RESULTS
	FROM
	(
		SELECT 
			DCBM.BLLG_ROLL_OVER_RATIO
			, DCBM.BLLG_FREE_COPY_CNT
			, DCBM.CUM_COPY_CNT
		FROM
			INV_PRT_FLEET_SMRY_MTR IPMTR,
			INV_PRT_FLEET_MACH IPMACH,
			INV_PRT_FLEET_SUB_TOT IPSUB,
			DS_CONTR_DTL DCD,
			DS_CONTR_BLLG_MTR DCBM,
			INV_PRT_FLEET_CHRG_DTL IPCD,
			DS_CONTR_BLLG_SCHD DCBS
		WHERE
			IPMTR.EZCANCELFLAG = '0'
		AND IPMTR.GLBL_CMPY_CD = ${P_GLBL_CMPY_CD}
		AND IPMTR.GLBL_CMPY_CD = IPSUB.GLBL_CMPY_CD
		AND IPMTR.INV_PRT_FLEET_SUB_TOT_PK = IPSUB.INV_PRT_FLEET_SUB_TOT_PK
		AND IPMTR.INV_NUM = ${P_INV_BILL_NUM}
		AND IPMACH.EZCANCELFLAG = '0'
		AND IPMTR.GLBL_CMPY_CD = IPMACH.GLBL_CMPY_CD
		AND IPMTR.INV_NUM = IPMACH.INV_NUM
		AND IPSUB.EZCANCELFLAG = '0'
		AND DCD.EZCANCELFLAG = '0'
		AND IPSUB.GLBL_CMPY_CD = DCD.GLBL_CMPY_CD
		AND IPSUB.DS_CONTR_PK = DCD.DS_CONTR_PK
		AND DCBM.EZCANCELFLAG = '0'
		AND DCD.GLBL_CMPY_CD = DCBM.GLBL_CMPY_CD
		AND DCD.DS_CONTR_DTL_PK = DCBM.DS_CONTR_DTL_PK
		AND IPSUB.GLBL_CMPY_CD = IPCD.GLBL_CMPY_CD
		AND IPCD.EZCANCELFLAG = '0'
		AND IPSUB.INV_PRT_FLEET_SUB_TOT_PK = IPCD.INV_PRT_FLEET_SUB_TOT_PK
		AND IPMTR.INV_NUM = IPCD.INV_NUM
		AND IPCD.LINE_TOT_AMT_TXT IS NULL
		AND IPCD.GLBL_CMPY_CD = DCBS.GLBL_CMPY_CD
		AND DCBS.EZCANCELFLAG = '0'
		AND IPMTR.INV_NUM = DCBS.SVC_INV_NUM
		AND DCBS.DS_CONTR_BLLG_MTR_PK = DCBM.DS_CONTR_BLLG_MTR_PK
		AND ((DCBM.BLLG_ROLL_OVER_RATIO IS NOT NULL AND DCBM.BLLG_ROLL_OVER_RATIO &lt;&gt; 0)
			OR (DCBM.BLLG_FREE_COPY_CNT IS NOT NULL AND DCBM.CUM_COPY_CNT IS NULL)
			OR (DCBM.CUM_COPY_CNT IS NOT NULL
				AND DCBM.CUM_COPY_START_DT &lt;= DCBS.BLLG_SCHD_THRU_DT
				AND DCBM.CUM_COPY_END_DT &gt;= DCBS.BLLG_SCHD_THRU_DT)
		)
	)
