	<!-- 3.Get Cover Data -->
	<variable name="sql3" type="sql" value="
		SELECT 
           'sql3' 																			sqlid
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN ${C22659} ELSE CASE WHEN I.INV_TP_CD = '2' THEN ${C1153} ELSE ${C2204} END  END) 			LB_TITLE
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN ${C22660} ELSE ${C2227} END)				LB_HDR_TXT1
		   ,H.INV_BILL_NUM 																	HDR_TXT1
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN ${C22661} ELSE ${C2216} END)				LB_HDR_TXT2
		   ,INITCAP(rtrim(to_char(to_date(H.INV_DT_TXT, 'MM/DD/YYYY'), 'MONTH'))) 
				|| ' ' || 
			INITCAP(to_char(to_date(H.INV_DT_TXT, 'MM/DD/YYYY'), 'DD, YYYY'))				HDR_TXT2
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN ${C22662} ELSE ${C2209} END)				LB_HDR_TXT3
		   ,H.INV_TOT_AMT_TXT 																HDR_TXT3
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN ${C22663} ELSE ${C22664} END)			LB_HDR_TXT4
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN H.INV_BAL_AMT_TXT ELSE NULL END)			HDR_TXT4_AMT
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN NULL 
			ELSE 
				INITCAP(rtrim(to_char(to_date(H.NET_DUE_DT_TXT, 'MM/DD/YYYY'), 'MONTH'))) 
					|| ' ' || 
				INITCAP(to_char(to_date(H.NET_DUE_DT_TXT, 'MM/DD/YYYY'), 'DD, YYYY')) END)	HDR_TXT4_DT
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN NULL ELSE ${C22663} END)					LB_HDR_TXT5
		   ,(CASE WHEN ${P_CONSL_BILL_FLG} = 'Y' THEN NULL ELSE H.INV_BAL_AMT_TXT END)			HDR_TXT5
		   ,regexp_replace(
		    	SUBSTR(H.FIRST_USG_INV_DESC_TXT, 0, 49) || CHR(46) || CHR (13) || CHR (10) || 
		    	H.SCD_USG_INV_DESC_TXT || CHR (13) || CHR (10) || 
		    	H.THIRD_USG_INV_DESC_TXT
		    , '('|| CHR (13) || CHR (10) || ')+$', '') 										USG_INV_DESC_TXT
		   ,H.INV_CMPY_FIRST_LINE_ADDR || H.INV_CMPY_SCD_LINE_PRNT_ADDR 					INV_CMPY_LINE_ADDR
		   ,H.INV_CMPY_CTY_ADDR || ', ' || H.INV_CMPY_ST_CD || ' ' || H.INV_CMPY_POST_CD	INV_CMPY_CTY_ADDR
		   ,H.INV_CMPY_TEL_NUM 																INV_CMPY_TEL_NUM
		   ,H.INV_CMPY_URL 																	INV_CMPY_URL
		   ,(select count(INV_PRT_FLEET_SUB_TOT_PK) from INV_PRT_FLEET_SUB_TOT 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = H.GLBL_CMPY_CD AND INV_BILL_NUM = H.INV_BILL_NUM
			AND INV_PRT_DTL_FMT_TP_CD = 'FL')												FLEET_CNT
		   ,(select count(INV_PRT_FLEET_SUB_TOT_PK) from INV_PRT_FLEET_SUB_TOT 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = H.GLBL_CMPY_CD AND INV_BILL_NUM = H.INV_BILL_NUM
			AND INV_PRT_DTL_FMT_TP_CD = 'AG')												AGGREGATE_CNT
		   ,(select count(INV_PRT_MAINT_SUB_TOT_PK) from INV_PRT_MAINT_SUB_TOT 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = H.GLBL_CMPY_CD AND INV_BILL_NUM = H.INV_BILL_NUM) 			MAINT_CNT
		   ,(select count(INV_PRT_SLS_PART_SUB_TOT_PK) from INV_PRT_SLS_PART_SUB_TOT 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = H.GLBL_CMPY_CD AND INV_BILL_NUM = H.INV_BILL_NUM) 			SLS_PART_CNT
		   ,(select count(INV_PRT_SLS_PART_SUB_TOT_PK) from INV_PRT_SLS_PART_SUB_TOT 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = H.GLBL_CMPY_CD AND NVL(CONSL_BILL_NUM,INV_NUM) = H.INV_BILL_NUM) 				SLS_PART_INV_CNT
		   ,CASE WHEN H.ESPAC_FLG = 'Y' THEN 1 ELSE 
           (select count(ORD_CATG_BIZ_CTX_PK) from ORD_CATG_BIZ_CTX 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = I.GLBL_CMPY_CD AND ORD_CATG_CTX_TP_CD = 'INV_PRT_SUPPLIES_ORDER'
            AND DS_ORD_CATG_CD = I.DS_ORD_CATG_CD
            AND (DS_ORD_TP_CD = I.DS_ORD_TP_CD OR DS_ORD_TP_CD IS NULL)) END 			SUPLY_CNT
		   ,(select count(ORD_CATG_BIZ_CTX_PK) from ORD_CATG_BIZ_CTX 
			where EZCANCELFLAG = '0' 
			AND GLBL_CMPY_CD = I.GLBL_CMPY_CD AND ORD_CATG_CTX_TP_CD = 'INV_PRT_PARTS_ORDER'
            AND DS_ORD_CATG_CD = I.DS_ORD_CATG_CD
            AND (DS_ORD_TP_CD = I.DS_ORD_TP_CD OR DS_ORD_TP_CD IS NULL)) 			PARTS_CNT
           ,'DUNS#: ' LB_DUNS_NUM
           ,CASE WHEN IPFD.LINE_BIZ_TP_CD IS NULL THEN
               CASE WHEN IPFD2.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN IPFD2.INV_PRINT_FTR_DESC_TXT_01 ELSE NULL END 
            ELSE
               CASE WHEN IPFD.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN IPFD.INV_PRINT_FTR_DESC_TXT_01 ELSE NULL END 
            END INV_PRINT_FTR_DESC_TXT_01
           ,CASE WHEN IPFD.LINE_BIZ_TP_CD IS NULL THEN
               CASE WHEN IPFD2.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN IPFD2.INV_PRINT_FTR_DESC_TXT_02 ELSE NULL END 
            ELSE
               CASE WHEN IPFD.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN IPFD.INV_PRINT_FTR_DESC_TXT_02 ELSE NULL END 
            END INV_PRINT_FTR_DESC_TXT_02
           ,CASE WHEN IPFD.LINE_BIZ_TP_CD IS NULL THEN
               CASE WHEN IPFD2.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN NULL ELSE IPFD2.INV_PRINT_FTR_DESC_TXT_01 || ' ' || IPFD2.INV_PRINT_FTR_DESC_TXT_02 END 
            ELSE
               CASE WHEN IPFD.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN NULL ELSE IPFD.INV_PRINT_FTR_DESC_TXT_01 || ' ' || IPFD.INV_PRINT_FTR_DESC_TXT_02 END 
            END INV_PRINT_FTR_DESC_TXT_03
           ,CASE WHEN IPFD.LINE_BIZ_TP_CD IS NULL THEN
               CASE WHEN IPFD2.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN '../../images/NWCF0100_MYCSA_IMAGE.png' ELSE NULL END 
            ELSE
               CASE WHEN IPFD.INV_PRINT_LINK_DPLY_FLG = 'Y' THEN '../../images/NWCF0100_MYCSA_IMAGE.png' ELSE NULL END 
            END IMAGE_FILE_PATH
           ,H.OCR_SCAN_NUM
		FROM 
		    INV_PRT_HDR H
           ,INV I
           ,INV_PRT_CTRL IPC
           ,SELL_TO_CUST STC
           ,PTY_LOC_WRK PLW
           ,INV_PRINT_FTR_DESC IPFD
           ,INV_PRINT_FTR_DESC IPFD2
		WHERE 
		    H.EZCANCELFLAG = '0'
		AND H.GLBL_CMPY_CD = ${P_GLBL_CMPY_CD}
		AND H.INV_BILL_NUM = ${P_INV_BILL_NUM}
		AND H.INV_PRT_CTRL_PK = ${P_INV_PRT_CTRL_PK}
