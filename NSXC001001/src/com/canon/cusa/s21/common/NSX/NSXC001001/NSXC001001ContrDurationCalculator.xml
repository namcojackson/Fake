<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="NSXC001001ContrDurationCalculator">

    <typeAlias alias="BLLG_CYCLE_UOMTMsg"   type="business.db.BLLG_CYCLE_UOMTMsg"/>

    <statement id="getCycleUomExceptDays" parameterClass="Map" resultMap="result.getCycleUomExceptDays">
        SELECT 
            GLBL_CMPY_CD
            ,BLLG_CYCLE_UOM_CD
            ,BLLG_CYCLE_UOM_NM
            ,BLLG_CYCLE_UOM_SORT_NUM
            ,BLLG_CYCLE_UOM_DESC_TXT
            ,BLLG_CYCLE_MTH_AOT
            ,BLLG_CYCLE_DAYS_AOT
            ,BLLG_CYCLE_MLY_FLG
            ,BLLG_CYCLE_DLY_FLG
            ,CALC_ORD_NUM
        FROM 
            BLLG_CYCLE_UOM A
        WHERE 
            A.GLBL_CMPY_CD      = #glblCmpyCd#
        AND A.BLLG_CYCLE_UOM_CD &lt;&gt; #days#
        AND A.EZCANCELFLAG      = '0'
        ORDER BY 
            A.CALC_ORD_NUM
    </statement>

    <resultMap id="result.getCycleUomExceptDays" class="BLLG_CYCLE_UOMTMsg">
        <result property="glblCmpyCd"           column="GLBL_CMPY_CD"               javaType="EZDTStringItem"/>
        <result property="bllgCycleUomCd"       column="BLLG_CYCLE_UOM_CD"          javaType="EZDTStringItem"/>
        <result property="bllgCycleUomNm"       column="BLLG_CYCLE_UOM_NM"          javaType="EZDTStringItem"/>
        <result property="bllgCycleUomSortNum"  column="BLLG_CYCLE_UOM_SORT_NUM"    javaType="EZDTBigDecimalItem"/>
        <result property="bllgCycleUomDescTxt"  column="BLLG_CYCLE_UOM_DESC_TXT"    javaType="EZDTStringItem"/>
        <result property="bllgCycleMthAot"      column="BLLG_CYCLE_MTH_AOT"         javaType="EZDTBigDecimalItem"/>
        <result property="bllgCycleDaysAot"     column="BLLG_CYCLE_DAYS_AOT"        javaType="EZDTBigDecimalItem"/>
        <result property="bllgCycleMlyFlg"      column="BLLG_CYCLE_MLY_FLG"         javaType="EZDTStringItem"/>
        <result property="bllgCycleDlyFlg"      column="BLLG_CYCLE_DLY_FLG"         javaType="EZDTStringItem"/>
        <result property="calcOrdNum"           column="CALC_ORD_NUM"               javaType="EZDTBigDecimalItem"/>
    </resultMap>
</sqlMap>
