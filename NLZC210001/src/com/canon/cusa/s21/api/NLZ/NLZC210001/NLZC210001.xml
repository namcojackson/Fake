<sqlMap namespace="NLZC210001">

    <!-- ==== Type Aliases ==== -->

    <!-- ==== Result Mapping ==== -->
    <statement id="getWhCdFromSWH" parameterClass="Map" resultMap="String">
        SELECT
            RS.RTL_WH_CD
        FROM
            RTL_SWH RS
        WHERE
            RS.GLBL_CMPY_CD = #glblCmpyCd#
        AND RS.INVTY_LOC_CD = #whCdAndSwhCd#
    </statement>
</sqlMap>
