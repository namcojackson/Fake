	<typeAlias alias="AR_RCPTTMsg_alias" type="business.db.AR_RCPTTMsg"/>
	<statement id="checkRcptExist" parameterClass="Map" resultMap="result.checkRcptExist">
     SELECT
               A.DEAL_RCPT_AMT
              ,A.DEAL_APPLY_AMT
              ,A.DEAL_APPLY_ADJ_AMT
