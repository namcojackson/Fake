     SELECT
               A.AR_TRX_BAL_PK
              ,A.AR_TRX_NUM
              ,A.AR_TRX_TP_CD
              ,A.DEAL_APPLY_GRS_AMT
              ,A.DEAL_APPLY_CASH_DISC_AMT
              ,A.DEAL_APPLY_CR_AMT
              ,A.DEAL_APPLY_ADJ_AMT
              ,A.DEAL_APPLY_ADJ_RSVD_AMT
              ,A.DEAL_RMNG_BAL_GRS_AMT
              ,A.BILL_TO_CUST_CD
              ,A.SELL_TO_CUST_CD
              ,A.PAYER_CUST_CD
              ,A.EZUPTIME
              ,A.EZUPTIMEZONE
     FROM
               AR_TRX_BAL  A
     WHERE
                    A.GLBL_CMPY_CD                  =  #glblCmpyCd#
               AND  A.GRP_INV_NUM                   =  #grpInvNum#
               AND  A.DEAL_RMNG_BAL_GRS_AMT  &lt;&gt;  0
               AND  A.EZCANCELFLAG                  =  '0'
     ORDER BY
                    A.GLBL_CMPY_CD
                   ,A.AR_TRX_NUM
