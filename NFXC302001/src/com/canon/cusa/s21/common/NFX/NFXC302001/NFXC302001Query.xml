<sqlMap namespace="NFXC302001Query">
	<typeAlias alias="AR_RCPTTMsg_alias" type="business.db.AR_RCPTTMsg"/>
    <typeAlias alias="CUST_CR_PRFLTMsg" type="business.db.CUST_CR_PRFLTMsg"/>


	<statement id="getArRcptEzdData" parameterClass="Map"  resultMap="result.AR_RCPTTMsg">
     SELECT
               A.EZINTIME       EZINTIME
              ,A.EZINTIMEZONE   EZINTIMEZONE
     FROM
              AR_RCPT  A 
     WHERE
                   A.GLBL_CMPY_CD  =  #GLBL_CMPY_CD#
              AND  A.RCPT_NUM      =  #RCPT_NUM#
	</statement>
	<resultMap id="result.AR_RCPTTMsg" class="AR_RCPTTMsg_alias">
	    <result property="ezInTime" column="EZINTIME" javaType="EZDTStringItem"/>
	    <result property="ezInTimeZone" column="EZINTIMEZONE" javaType="EZDTStringItem"/>
	</resultMap>
	
	<typeAlias alias="AR_TRX_BALTMsg_alias" type="business.db.AR_TRX_BALTMsg"/>
	<statement id="getArTrxBalEzdData" parameterClass="Map"  resultMap="result.AR_TRX_BALTMsg">
     SELECT
               A.EZINTIME       EZINTIME
              ,A.EZINTIMEZONE   EZINTIMEZONE
     FROM
               AR_TRX_BAL  A 
     WHERE
                    A.GLBL_CMPY_CD   =  #GLBL_CMPY_CD#
               AND  A.AR_TRX_BAL_PK  =  #AR_TRX_BAL_PK#
	</statement>
	<resultMap id="result.AR_TRX_BALTMsg" class="AR_TRX_BALTMsg_alias">
	    <result property="ezInTime" column="EZINTIME" javaType="EZDTStringItem"/>
	    <result property="ezInTimeZone" column="EZINTIMEZONE" javaType="EZDTStringItem"/>
	</resultMap>

