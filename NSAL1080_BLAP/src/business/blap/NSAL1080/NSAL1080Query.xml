<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NSAL1080Query">
    <typeAlias alias="NSAL1080CMsg"  type="business.blap.NSAL1080.NSAL1080CMsg"/>

    <statement id="getCIInfo" parameterClass="Map" resultMap="result.getCIInfo">
            SELECT
                 B.SVC_CR_REBIL_STS_DESC_TXT    AS  SVC_CR_REBIL_STS_DESC_TXT
                ,A.SVC_CR_REBIL_DESC_TXT        AS  SVC_CR_REBIL_DESC_TXT
                ,A.SVC_CR_REBIL_PK              AS  SVC_CR_REBIL_PK
            FROM
                 SVC_CR_REBIL         A
                ,SVC_CR_REBIL_STS     B
            WHERE
                    A.GLBL_CMPY_CD              = #glblCmpyCd#
                AND A.CUST_INCDT_ID             = #custIncdtId#
                AND A.EZCANCELFLAG              = '0'
                AND A.GLBL_CMPY_CD              = B.GLBL_CMPY_CD
                AND A.SVC_CR_REBIL_STS_CD       = B.SVC_CR_REBIL_STS_CD
                AND B.EZCANCELFLAG              = '0'
    </statement>

    <resultMap id="result.getCIInfo"      class="NSAL1080CMsg">
        <result property="svcCrRebilStsDescTxt" column="SVC_CR_REBIL_STS_DESC_TXT" javaType="EZDSStringItem" />
        <result property="svcCrRebilDescTxt"    column="SVC_CR_REBIL_DESC_TXT"     javaType="EZDSStringItem" />
        <result property="svcCrRebilPk"         column="SVC_CR_REBIL_PK"           javaType="EZDSBigDecimalItem" />
    </resultMap>
</sqlMap>
