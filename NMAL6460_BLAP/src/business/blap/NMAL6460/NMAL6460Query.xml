<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NMAL6460Query">

	<typeAlias alias="NMAL6460CMsg"		type="business.blap.NMAL6460.NMAL6460CMsg"/>
	<typeAlias alias="NMAL6460_ASMsg"	type="business.blap.NMAL6460.NMAL6460_ASMsg"/>

	<resultMap id="result.getNMAL6460"    class="NMAL6460_ASMsg">
		<result property="glblCmpyCd_A1"               column="GLBL_CMPY_CD"                  javaType="EZDCStringItem"/>
		<result property="pmtTermCashDiscSortNum_A1"   column="PMT_TERM_CASH_DISC_SORT_NUM"   javaType="EZDCBigDecimalItem"/>
		<result property="pmtTermCashDiscCd_A1"        column="PMT_TERM_CASH_DISC_CD"         javaType="EZDCStringItem"/>
		<result property="pmtTermCashDiscNm_A1"        column="PMT_TERM_CASH_DISC_NM"         javaType="EZDCStringItem"/>
		<result property="pmtTermCashDiscDescTxt_A1"   column="PMT_TERM_CASH_DISC_DESC_TXT"   javaType="EZDCStringItem"/>
		<result property="pmtTermCd_A1"                column="PMT_TERM_CD"                   javaType="EZDCStringItem"/>
		<result property="pmtTermNm_A1"                column="PMT_TERM_NM"                   javaType="EZDCStringItem"/>
		<result property="cashDiscTermCd_A1"           column="CASH_DISC_TERM_CD"             javaType="EZDCStringItem"/>
		<result property="ediCashDiscDueAot_A1"        column="EDI_CASH_DISC_DUE_AOT"         javaType="EZDCBigDecimalItem"/>
		<result property="ediCashDiscPct_A1"           column="EDI_CASH_DISC_PCT"             javaType="EZDCBigDecimalItem"/>
		<result property="pmtTermSendFaxFlg_A1"        column="PMT_TERM_SEND_FAX_FLG"         javaType="EZDCStringItem"/>
		<result property="istlPmtTermFlg_A1"           column="ISTL_PMT_TERM_FLG"             javaType="EZDCStringItem"/>
		<result property="actlExchRateTpNum_A1"        column="ACTL_EXCH_RATE_TP_NUM"         javaType="EZDCStringItem"/>
		<result property="pmtTermConslFlg_A1"          column="PMT_TERM_CONSL_FLG"            javaType="EZDCStringItem"/>
		<result property="pmtTermConslLastDomFlg_A1"   column="PMT_TERM_CONSL_LAST_DOM_FLG"   javaType="EZDCStringItem"/>
		<result property="pmtTermConslDueDay_A1"       column="PMT_TERM_CONSL_DUE_DAY"        javaType="EZDCStringItem"/>
		<result property="pmtTermConslAddMthNum_A1"    column="PMT_TERM_CONSL_ADD_MTH_NUM"    javaType="EZDCBigDecimalItem"/>
		<result property="pmtCashFlg_A1"               column="PMT_CASH_FLG"                  javaType="EZDCStringItem"/>
		<result property="pmtCcFlg_A1"                 column="PMT_CC_FLG"                    javaType="EZDCStringItem"/>
		<result property="ezUpTime_A1"                 column="EZUPTIME"                      javaType="EZDCStringItem"/>
		<result property="ezUpTimeZone_A1"             column="EZUPTIMEZONE"                  javaType="EZDCStringItem"/>
	</resultMap>
	
    <statement id="getCodeTable" parameterClass="NMAL6460CMsg" resultMap="result.getNMAL6460">
        SELECT 
            PTCD.GLBL_CMPY_CD                      GLBL_CMPY_CD
           ,PTCD.PMT_TERM_CASH_DISC_CD             PMT_TERM_CASH_DISC_CD
           ,PTCD.PMT_TERM_CASH_DISC_NM             PMT_TERM_CASH_DISC_NM
           ,PTCD.PMT_TERM_CASH_DISC_SORT_NUM       PMT_TERM_CASH_DISC_SORT_NUM
           ,PTCD.PMT_TERM_CASH_DISC_DESC_TXT       PMT_TERM_CASH_DISC_DESC_TXT
           ,PTCD.PMT_TERM_CD                       PMT_TERM_CD
           ,PT.PMT_TERM_NM                         PMT_TERM_NM
           ,PTCD.CASH_DISC_TERM_CD                 CASH_DISC_TERM_CD
           ,PTCD.EDI_CASH_DISC_DUE_AOT             EDI_CASH_DISC_DUE_AOT
           ,PTCD.EDI_CASH_DISC_PCT                 EDI_CASH_DISC_PCT
           ,PTCD.PMT_TERM_SEND_FAX_FLG             PMT_TERM_SEND_FAX_FLG
           ,PTCD.ISTL_PMT_TERM_FLG                 ISTL_PMT_TERM_FLG
           ,PTCD.ACTL_EXCH_RATE_TP_NUM             ACTL_EXCH_RATE_TP_NUM
           ,PMT_TERM_CONSL_FLG                     PMT_TERM_CONSL_FLG
           ,PMT_TERM_CONSL_LAST_DOM_FLG            PMT_TERM_CONSL_LAST_DOM_FLG
           ,PMT_TERM_CONSL_DUE_DAY                 PMT_TERM_CONSL_DUE_DAY
           ,PMT_TERM_CONSL_ADD_MTH_NUM             PMT_TERM_CONSL_ADD_MTH_NUM
           ,PMT_CASH_FLG                           PMT_CASH_FLG
           ,PMT_CC_FLG                             PMT_CC_FLG
           ,NVL(PTCD.EZUPTIME, '*')                EZUPTIME
		   ,NVL(PTCD.EZUPTIMEZONE, '*')            EZUPTIMEZONE
        FROM 
            PMT_TERM_CASH_DISC    PTCD
           ,PMT_TERM              PT
        WHERE
                 PTCD.GLBL_CMPY_CD = #glblCmpyCd#
            AND  PTCD.GLBL_CMPY_CD = PT.GLBL_CMPY_CD
            AND  PTCD.PMT_TERM_CD  = PT.PMT_TERM_CD
            AND  PTCD.EZCANCELFLAG = '0'
        ORDER BY
            PTCD.PMT_TERM_CASH_DISC_SORT_NUM
    </statement>
</sqlMap>
