<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NMAL6370Query">

	<typeAlias alias="NMAL6370CMsg"		type="business.blap.NMAL6370.NMAL6370CMsg"/>
	<typeAlias alias="NMAL6370_ASMsg"	type="business.blap.NMAL6370.NMAL6370_ASMsg"/>

	<resultMap id="result.getNMAL6370"    class="NMAL6370_ASMsg">
		<result property="glblCmpyCd_A1"      column="GLBL_CMPY_CD"       javaType="EZDCStringItem"/>
		<result property="ctacTpSortNum_A1"   column="CTAC_TP_SORT_NUM"   javaType="EZDCBigDecimalItem"/>
		<result property="ctacTpCd_A1"        column="CTAC_TP_CD"         javaType="EZDCStringItem"/>
		<result property="ctacTpNm_A1"        column="CTAC_TP_NM"         javaType="EZDCStringItem"/>
		<result property="ctacTpDescTxt_A1"   column="CTAC_TP_DESC_TXT"   javaType="EZDCStringItem"/>
		<result property="techFlg_A1"         column="TECH_FLG"           javaType="EZDCStringItem"/>
		<result property="actvFlg_A1"         column="ACTV_FLG"           javaType="EZDCStringItem"/>
		<result property="execFlg_A1"         column="EXEC_FLG"           javaType="EZDCStringItem"/>
		<result property="svcIndFlg_A1"       column="SVC_IND_FLG"        javaType="EZDCStringItem"/>
		<result property="ezUpTime_A1"        column="EZUPTIME"           javaType="EZDCStringItem"/>
		<result property="ezUpTimeZone_A1"    column="EZUPTIMEZONE"       javaType="EZDCStringItem"/>
	</resultMap>
	
    <statement id="getCodeTable" parameterClass="NMAL6370CMsg" resultMap="result.getNMAL6370">
        SELECT 
            CT.GLBL_CMPY_CD           GLBL_CMPY_CD
           ,CT.CTAC_TP_CD             CTAC_TP_CD
           ,CT.CTAC_TP_NM             CTAC_TP_NM
           ,CT.CTAC_TP_SORT_NUM       CTAC_TP_SORT_NUM
           ,CT.CTAC_TP_DESC_TXT       CTAC_TP_DESC_TXT
           ,CT.TECH_FLG               TECH_FLG
           ,CT.ACTV_FLG               ACTV_FLG
           ,CT.EXEC_FLG               EXEC_FLG
           ,CT.SVC_IND_FLG            SVC_IND_FLG
           ,NVL(CT.EZUPTIME, '*')     EZUPTIME
		   ,NVL(CT.EZUPTIMEZONE, '*') EZUPTIMEZONE
        FROM 
            CTAC_TP    CT
        WHERE
                 CT.GLBL_CMPY_CD = #glblCmpyCd#
            AND  CT.EZCANCELFLAG = '0'
        ORDER BY
            CT.CTAC_TP_SORT_NUM
    </statement>
	
</sqlMap>
