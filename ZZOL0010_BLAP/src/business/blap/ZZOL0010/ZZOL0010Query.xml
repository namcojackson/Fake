<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="ZZOL0010Query">

	<typeAlias alias="ZZOL0010CMsg"		type="business.blap.ZZOL0010.ZZOL0010CMsg"/>
	<typeAlias alias="ZZOL0010_ASMsg"	type="business.blap.ZZOL0010.ZZOL0010_ASMsg"/>
	
	<statement id="getAOM02Data" parameterClass="Map" resultMap="result.getAOM02Data">
		SELECT
	        EZBUSINESSID
	    ,   EZCOMPANYCODE
	    ,   EZONLSTARTTIME
	    ,   EZONLENDTIME
	    ,   EZONLSTOPFLAG
	    ,	EZACCTINFOMODE
	    ,   EZDEBUGLEVEL
	    ,	EZUPTIMEZONE
	    ,	EZUPTIME
		FROM
		        AOM02

		WHERE
			EZCANCELFLAG = 0 
		<isNotNull property="cMsg.ezBusinessID_SB">
			AND EZBUSINESSID LIKE #cMsg.ezBusinessID_SB#
		</isNotNull>
		
		<isNotNull property="cMsg.ezCompanyCode_SB">
			AND EZCOMPANYCODE = #cMsg.ezCompanyCode_SB#
		</isNotNull>
		
		<isNotNull property="cMsg.ezOnlStopFlag_SB">
			AND EZONLSTOPFLAG =  #cMsg.ezOnlStopFlag_SB#
		</isNotNull>
		
		<isNotNull property="cMsg.ezAcctInfoMode_SB">
			AND EZACCTINFOMODE = #cMsg.ezAcctInfoMode_SB#
		</isNotNull>
		
		<isNotNull property="cMsg.ezDebugLevel_SB">
			AND EZDEBUGLEVEL = #cMsg.ezDebugLevel_SB#
		</isNotNull>
		
		ORDER BY
			EZBUSINESSID ASC,
			EZCOMPANYCODE ASC,
			EZONLSTOPFLAG ASC
			
	</statement>
	
	<resultMap id="result.getAOM02Data"     class="ZZOL0010_ASMsg">
		<result property="ezBusinessID_A1"		column="EZBUSINESSID"        javaType="EZDSStringItem"/>
		<result property="ezCompanyCode_A1"		column="EZCOMPANYCODE"       javaType="EZDSStringItem"/>
		<result property="xxHrsMnScd_AS"		column="EZONLSTARTTIME"      javaType="EZDSStringItem"/>
		<result property="xxHrsMnScd_AE"		column="EZONLENDTIME"        javaType="EZDSStringItem"/>
		<result property="ezOnlStopFlag_A1"		column="EZONLSTOPFLAG"       javaType="EZDSStringItem"/>
		<result property="ezAcctInfoMode_A1"	column="EZACCTINFOMODE"      javaType="EZDSStringItem"/>
		<result property="ezDebugLevel_A1"      column="EZDEBUGLEVEL"        javaType="EZDSStringItem"/>
		<result property="ezUpTime_A1"      	column="EZUPTIME"      		 javaType="EZDSStringItem"/>
		<result property="ezUpTimeZone_A1"      column="EZUPTIMEZONE"        javaType="EZDSStringItem"/>
	</resultMap>

</sqlMap>
