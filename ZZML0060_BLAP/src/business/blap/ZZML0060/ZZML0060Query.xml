<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="ZZML0060Query">

    <typeAlias alias="ZZML0060_ASMsg" type="business.blap.ZZML0060.ZZML0060_ASMsg"/>
    <typeAlias alias="ZZML0060CMsg"   type="business.blap.ZZML0060.ZZML0060CMsg"/>

    <resultMap id="result.ZZML0060_ASMsg" class="ZZML0060_ASMsg">
        <result property="ezUpTime_A"      column="EZUPTIME"          javaType="EZDCStringItem"/>
        <result property="ezUpTimeZone_A"  column="EZUPTIMEZONE"      javaType="EZDCStringItem"/>
        <result property="mlGrpId_A"       column="ML_GRP_ID"         javaType="EZDCStringItem"/>
        <result property="mlGrpNm_A"       column="ML_GRP_NM"         javaType="EZDCStringItem"/>
        <result property="mlKeyFirstNm_A"  column="ML_KEY_FIRST_NM"   javaType="EZDCStringItem"/>
        <result property="mlKeyScdNm_A"    column="ML_KEY_SCD_NM"     javaType="EZDCStringItem"/>
        <result property="mlKeyThirdNm_A"  column="ML_KEY_THIRD_NM"   javaType="EZDCStringItem"/>
        <result property="mlGrpDescTxt_A"  column="ML_GRP_DESC_TXT"   javaType="EZDCStringItem"/>
    </resultMap>

    <resultMap id="result.ZZML0060CMsg" class="ZZML0060CMsg">
        <result property="ezUpTime_01"     column="EZUPTIME"          javaType="EZDCStringItem"/>
        <result property="ezUpTimeZone_01" column="EZUPTIMEZONE"      javaType="EZDCStringItem"/>
        <result property="glblCmpyCd_01"   column="GLBL_CMPY_CD"      javaType="EZDCStringItem"/>
        <result property="mlGrpId_01"      column="ML_GRP_ID"         javaType="EZDCStringItem"/>
        <result property="mlGrpNm_01"      column="ML_GRP_NM"         javaType="EZDCStringItem"/>
        <result property="mlKeyFirstNm_01" column="ML_KEY_FIRST_NM"   javaType="EZDCStringItem"/>
        <result property="mlKeyScdNm_01"   column="ML_KEY_SCD_NM"     javaType="EZDCStringItem"/>
        <result property="mlKeyThirdNm_01" column="ML_KEY_THIRD_NM"   javaType="EZDCStringItem"/>
        <result property="mlGrpDescTxt_01" column="ML_GRP_DESC_TXT"   javaType="EZDCStringItem"/>
    </resultMap>

    <statement id="getMlGrpAddr" parameterClass="Map" resultMap="result.ZZML0060_ASMsg">
      SELECT
        EZUPTIME,
        EZUPTIMEZONE,
        GLBL_CMPY_CD,
        ML_GRP_ID,
        ML_GRP_NM,
        ML_KEY_FIRST_NM,
        ML_KEY_SCD_NM,
        ML_KEY_THIRD_NM,
        ML_GRP_DESC_TXT
      FROM
        ML_GRP_ADDR
      WHERE
        EZCANCELFLAG     = '0'
        AND GLBL_CMPY_CD = #glblCmpyCd#
      <isNotNull property="mlGrpId">
        AND ML_GRP_ID LIKE #mlGrpId#
      </isNotNull>
      <isNotNull property="mlGrpNm">
        AND ML_GRP_NM LIKE #mlGrpNm#
      </isNotNull>
      ORDER BY
        ML_GRP_ID
    </statement>

    <statement id="getMlGrpAddrByKey" parameterClass="Map" resultMap="result.ZZML0060CMsg">
      SELECT
        EZUPTIME,
        EZUPTIMEZONE,
        GLBL_CMPY_CD,
        ML_GRP_ID,
        ML_GRP_NM,
        ML_KEY_FIRST_NM,
        ML_KEY_SCD_NM,
        ML_KEY_THIRD_NM,
        ML_GRP_DESC_TXT
      FROM
        ML_GRP_ADDR
      WHERE
        EZCANCELFLAG     = '0'
        AND GLBL_CMPY_CD = #glblCmpyCd#
        AND ML_GRP_ID    = #mlGrpId#
    </statement>
</sqlMap>
