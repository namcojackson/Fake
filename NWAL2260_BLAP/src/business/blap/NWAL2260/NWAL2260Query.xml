    <statement id="getSearchDataForDetail" parameterClass="Map" resultMap="result.getSearchDataForDetail">
        SELECT
             DDA.ORD_SRC_REF_LINE_NUM
            ,DDA.ORD_SRC_REF_LINE_SUB_NUM
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_01
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_02
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_03
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_04
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_05
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_06
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_07
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_08
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_09
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_10
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_11
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_12
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_13
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_14
            ,DDA.DS_IMPT_DTL_ATTRB_TXT_15
            ,DDA.IDOC_PO_DTL_LINE_REF_NUM
            ,DDA.IDOC_PO_DTL_MAT_NUM_01
            ,DDA.IDOC_PO_DTL_MAT_NUM_02
            ,DDA.IDOC_PO_DTL_ORD_QTY
            ,DDA.IDOC_PO_DTL_UOM_CD
            ,DDA.IDOC_PO_DTL_DELY_PRTY_NM
            ,DDA.EZUPTIME
            ,DDA.EZUPTIMEZONE
            ,DDC.SCR_LB_NM_01
            ,DDC.SCR_LB_NM_02
            ,DDC.SCR_LB_NM_03
            ,DDC.SCR_LB_NM_04
            ,DDC.SCR_LB_NM_05
            ,DDC.SCR_LB_NM_06
            ,DDC.SCR_LB_NM_07
            ,DDC.SCR_LB_NM_08
            ,DDC.SCR_LB_NM_09
            ,DDC.SCR_LB_NM_10
            ,DDC.SCR_LB_NM_11
            ,DDC.SCR_LB_NM_12
            ,DDC.SCR_LB_NM_13
            ,DDC.SCR_LB_NM_14
            ,DDC.SCR_LB_NM_15
            ,CD.DS_ORD_POSN_NUM
            ,CD.DS_CPO_LINE_NUM
            ,CP1.CPO_ORD_NUM
        FROM
             DS_IMPT_DTL_EXT_ATTRB  DDA
            ,DS_IMPT_DTL_ATTRB_CTRL DDC
            ,DS_IMPT_EXT_ATTRB      DEA
            <!-- 2018/07/03 S21_NA#26908 Add Start -->
            ,DS_IMPT_ORD_DTL        DOD
            <!-- 2018/07/03 S21_NA#26908 Add End -->
            ,CPO_DTL                CD
            ,CPO                    CP1
        WHERE
                DDA.GLBL_CMPY_CD                = #glblCmpyCd#
            AND DDA.DS_IMPT_ORD_DTL_PK          = #dsImptordDtlPk#
            AND DDA.EZCANCELFLAG                = '0'
            AND DDA.GLBL_CMPY_CD                = DDC.GLBL_CMPY_CD(+)
            AND DDA.ATTRB_CTRL_ID               = DDC.ATTRB_CTRL_ID(+)
            AND DDC.EZCANCELFLAG(+)             = '0'
            AND DDA.GLBL_CMPY_CD                = DEA.GLBL_CMPY_CD
            AND DDA.DS_IMPT_ORD_PK              = DEA.DS_IMPT_ORD_PK
            AND DEA.EZCANCELFLAG                = '0'
            AND DEA.GLBL_CMPY_CD                = CP1.GLBL_CMPY_CD(+)
            AND DEA.ORD_SRC_REF_NUM             = CP1.ORD_SRC_REF_NUM(+)
            AND CP1.EZCANCELFLAG(+)             = '0'
            <!-- 2018/07/03 S21_NA#26908 Add Start -->
            AND DDA.GLBL_CMPY_CD                = DOD.GLBL_CMPY_CD(+)
            AND DDA.DS_IMPT_ORD_DTL_PK          = DOD.DS_IMPT_ORD_DTL_PK(+)
            AND DOD.EZCANCELFLAG(+)             = '0'
            <!-- 2018/07/03 S21_NA#26908 Add End -->
            AND CP1.GLBL_CMPY_CD                = CD.GLBL_CMPY_CD(+)
            AND CP1.CPO_ORD_NUM                 = CD.CPO_ORD_NUM(+)
            <!-- 2018/07/03 S21_NA#26908 Mod Start -->
            <!-- AND DDA.ORD_SRC_REF_LINE_NUM        = CD.ORD_SRC_REF_LINE_NUM(+) -->
            <!-- AND DDA.ORD_SRC_REF_LINE_SUB_NUM    = CD.ORD_SRC_REF_LINE_SUB_NUM(+) -->
            AND DOD.ORD_SRC_REF_LINE_NUM        = CD.ORD_SRC_REF_LINE_NUM(+)
            AND DOD.ORD_SRC_REF_LINE_SUB_NUM    = CD.ORD_SRC_REF_LINE_SUB_NUM(+)
            <!-- 2018/07/03 S21_NA#26908 Mod End -->
            AND CD.EZCANCELFLAG(+)             = '0'
    </statement>
