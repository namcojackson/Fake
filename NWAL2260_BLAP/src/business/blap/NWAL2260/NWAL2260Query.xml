                DEA.GLBL_CMPY_CD     = #glblCmpyCd#
        <isNotNull property="ordSrcRefNum">
            AND DEA.ORD_SRC_REF_NUM  = #ordSrcRefNum#
            AND DEA.CPO_SRC_TP_CD    = #cpoSrcTpCd#
        </isNotNull>
        <isNotNull property="dsImptOrdPk">
            AND DEA.DS_IMPT_ORD_PK   = #dsImptOrdPk#
        </isNotNull>
            AND DEA.EZCANCELFLAG     = '0'
            AND DEA.GLBL_CMPY_CD     = DAC.GLBL_CMPY_CD(+)
            AND DEA.ATTRB_CTRL_ID    = DAC.ATTRB_CTRL_ID(+)
            AND DAC.EZCANCELFLAG(+)  = '0'
            AND DEA.GLBL_CMPY_CD     = CP1.GLBL_CMPY_CD(+)
            AND DEA.ORD_SRC_REF_NUM  = CP1.ORD_SRC_REF_NUM(+)
            AND CP1.EZCANCELFLAG(+)  = '0'
            <!-- 2018/07/03 S21_NA#26908 Add Start -->
            AND DEA.GLBL_CMPY_CD     = ETP.GLBL_CMPY_CD(+)
            AND DEA.EDI_TRD_PTNR_CD  = ETP.DS_EDI_TRD_PTNR_CD(+)
            AND ETP.EZCANCELFLAG(+)  = '0'
