<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="ZZOL0050Query">

	<typeAlias alias="ZZOL0050_ASMsg"	type="business.blap.ZZOL0050.ZZOL0050_ASMsg"/>

	<!-- CSV Upload config search -->
	<statement id="getList" parameterClass="Map" resultMap="result.ZZOL0050_ASMsg">
		SELECT
				UPLD_CSV_ID
			,	UPLD_CSV_NM
			,	UPLD_CSV_FILE_ID
			,	UPLD_CSV_TEMP_TBL_ID
			,	EZREQBUSINESSID
			,	EZUPTIME
			,	EZUPTIMEZONE
		FROM
		        UPLD_CSV_MSTR
		WHERE
				EZCANCELFLAG  = '0'
			AND GLBL_CMPY_CD  = #glblCmpyCd#

		<!-- Upload CSV ID -->
		<isNotNull property="upldCsvId">
			AND UPLD_CSV_ID LIKE #upldCsvId#
		</isNotNull>

		<!-- Upload CSV  Name -->
		<isNotNull property="upldCsvNm">
			AND UPLD_CSV_NM LIKE #upldCsvNm#
		</isNotNull>
		ORDER BY
			UPLD_CSV_ID
	
	</statement>
	
	<resultMap id="result.ZZOL0050_ASMsg" 		class="ZZOL0050_ASMsg">
		<result property="upldCsvId_A"			column="UPLD_CSV_ID"			javaType="EZDSStringItem"/>
		<result property="upldCsvNm_A"			column="UPLD_CSV_NM"			javaType="EZDSStringItem"/>
		<result property="upldCsvFileId_A"		column="UPLD_CSV_FILE_ID"		javaType="EZDSStringItem"/>
		<result property="upldCsvTempTblId_A"	column="UPLD_CSV_TEMP_TBL_ID"	javaType="EZDSStringItem"/>
		<result property="ezReqBusinessID_A"	column="EZREQBUSINESSID"		javaType="EZDSStringItem"/>
		<result property="ezUpTime_A"			column="EZUPTIME"				javaType="EZDSStringItem"/>
		<result property="ezUpTimeZone_A"		column="EZUPTIMEZONE"			javaType="EZDSStringItem"/>
	</resultMap>
	
</sqlMap>