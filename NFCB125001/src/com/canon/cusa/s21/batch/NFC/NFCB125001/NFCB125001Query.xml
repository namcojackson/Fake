<?xml version="1.0" encoding="UTF-8"?>
<sqlMap namespace="NFCB125001Query">

    <typeAlias alias="AR_RCPTTMsg" type="business.db.AR_RCPTTMsg"/>
    <statement id="getFinalRcptDt" parameterClass="Map" resultMap="result.AR_RCPTTMsg">
     SELECT
                MAX(A.RCPT_DT)   RCPT_DT
               ,MAX(A.GL_DT)     GL_DT
     FROM
               AR_RCPT  A
     WHERE
                   A.GLBL_CMPY_CD   =  #glblCmpyCd#
               AND A.PAYER_CUST_CD  =  #billToCustCd#
               AND A.DEAL_RCPT_AMT  != 0
               AND A.EZCANCELFLAG   =  '0'
    </statement>
    <!-- AR_RCPTTMsg Mapping -->
    <resultMap id="result.AR_RCPTTMsg" class="AR_RCPTTMsg">
        <result property="rcptDt" column="RCPT_DT" javaType="EZDTStringItem"/>
        <result property="glDt" column="GL_DT" javaType="EZDTStringItem"/>
    </resultMap>
</sqlMap>
