<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NMAL6670Query">

	<typeAlias alias="NMAL6670CMsg"		type="business.blap.NMAL6670.NMAL6670CMsg"/>
	<typeAlias alias="NMAL6670_ASMsg"	type="business.blap.NMAL6670.NMAL6670_ASMsg"/>

	<resultMap id="result.getNMAL6670"    class="NMAL6670_ASMsg">
		<result property="glblCmpyCd_A1"         column="GLBL_CMPY_CD"          javaType="EZDCStringItem"/>
		<result property="istlPmtTermCd_A1"      column="ISTL_PMT_TERM_CD"      javaType="EZDCStringItem"/>
		<result property="istlPmtTermDtlNum_A1"  column="ISTL_PMT_TERM_DTL_NUM" javaType="EZDCStringItem"/>
		<result property="istlPmtTermAot_A1"     column="ISTL_PMT_TERM_AOT"     javaType="EZDCBigDecimalItem"/>
		<result property="istlPmtTermPct_A1"     column="ISTL_PMT_TERM_PCT"     javaType="EZDCBigDecimalItem"/>
		<result property="ezUpTime_A1"         column="EZUPTIME"            javaType="EZDCStringItem"/>
		<result property="ezUpTimeZone_A1"     column="EZUPTIMEZONE"        javaType="EZDCStringItem"/>
	</resultMap>
	
    <statement id="getCodeTable" parameterClass="NMAL6670CMsg" resultMap="result.getNMAL6670">
        SELECT 
            IPT.GLBL_CMPY_CD            GLBL_CMPY_CD
           ,IPT.ISTL_PMT_TERM_CD        ISTL_PMT_TERM_CD
           ,IPT.ISTL_PMT_TERM_DTL_NUM   ISTL_PMT_TERM_DTL_NUM
           ,IPT.ISTL_PMT_TERM_AOT       ISTL_PMT_TERM_AOT
           ,IPT.ISTL_PMT_TERM_PCT       ISTL_PMT_TERM_PCT
           ,NVL(IPT.EZUPTIME, '*')      EZUPTIME
		   ,NVL(IPT.EZUPTIMEZONE, '*')  EZUPTIMEZONE
        FROM 
            ISTL_PMT_TERM    IPT
        WHERE
                 IPT.GLBL_CMPY_CD = #glblCmpyCd#
            AND  IPT.EZCANCELFLAG = '0'
        ORDER BY
            IPT.ISTL_PMT_TERM_CD
           ,IPT.ISTL_PMT_TERM_DTL_NUM
    </statement>
</sqlMap>
