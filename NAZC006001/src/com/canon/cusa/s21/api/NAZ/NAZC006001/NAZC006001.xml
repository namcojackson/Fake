    <statement id="searchHoldCnt" parameterClass="Map" resultClass="Map">
        SELECT
                  H.CPO_DTL_LINE_NUM
                 ,H.CPO_DTL_LINE_SUB_NUM
                 ,H.SHPG_PLN_NUM
                 ,H.HLD_RSN_CD
                 ,H.REL_FLG
                 ,COUNT(*) AS CNT
                   
        FROM
                 HLD H
                  
        WHERE
                 H.GLBL_CMPY_CD                = #glblCmpyCd#
          AND    H.TRX_SRC_TP_CD               = #trxSrcTpCd#
          AND    H.CPO_ORD_NUM                 = #cpoOrdNum#
          AND    H.EZCANCELFLAG                = '0'
       GROUP BY
                  H.CPO_DTL_LINE_NUM
                 ,H.CPO_DTL_LINE_SUB_NUM
                 ,H.SHPG_PLN_NUM
                 ,H.HLD_RSN_CD
                 ,H.REL_FLG
    </statement>


    <statement id="getShpgPln" parameterClass="Map" resultMap="result.SHPG_PLNTMsg">
    SELECT
        *
    FROM
       SHPG_PLN SP
    WHERE
           SP.EZCANCELFLAG = '0'
       AND SP.SHPG_STS_CD  != #shpgStsCd#
       AND SP.GLBL_CMPY_CD = #glblCmpyCd#
       AND SP.TRX_HDR_NUM = (
           SELECT
               TRX_HDR_NUM
           FROM
               SHPG_PLN SP1
           WHERE
                   SP1.SHPG_PLN_NUM = #shpgPlnNum#
               AND SP1.GLBL_CMPY_CD = #glblCmpyCd#
               AND SP1.EZCANCELFLAG = '0'
               AND ROWNUM = 1)
    </statement>
