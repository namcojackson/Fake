<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NLGL0060Query">

    <typeAlias alias="NLGL0060_ASMsg"      type="business.blap.NLGL0060.NLGL0060_ASMsg"/>

    <statement id="getTaskList" parameterClass="Map" resultClass="Map">
        SELECT
                WT.WMS_TASK_CD               AS  WMS_TASK_CD
              , WT.WMS_TASK_NM               AS  WMS_TASK_NM
          FROM
                WMS_TASK                     WT
         WHERE
                WT.GLBL_CMPY_CD            = #glblCmpyCd#
           AND  WT.EZCANCELFLAG            = '0'
      ORDER BY
                WT.WMS_TASK_SORT_NUM
    </statement>

    <statement id="search" parameterClass="Map" resultMap="result.NLGL0060_ASMsg">
        SELECT
                WIT.WMS_INBD_TRX_PK          AS  WMS_INBD_TRX_PK
              , WIT.EZINTIME                 AS  EZINTIME
              , WIT.EZUPTIME                 AS  EZUPTIME
              , WIT.WH_CD                    AS  WH_CD
              , WIT.WMS_TASK_CD              AS  WMS_TASK_CD
              , WIT.PROC_STS_CD              AS  PROC_STS_CD
              , WIT.ERR_MSG_CD               AS  ERR_MSG_CD
              , WIT.WMS_MDSE_CD              AS  WMS_MDSE_CD
              , WIT.SER_NUM                  AS  SER_NUM
              , WIT.WMS_ORD_QTY              AS  WMS_ORD_QTY
              , WIT.INBD_ORD_NUM             AS  INBD_ORD_NUM
              , WIT.OTBD_ORD_NUM             AS  OTBD_ORD_NUM
              , WIT.EZUPTIMEZONE             AS  EZUPTIMEZONE
          FROM
               (
                SELECT
                        WIT.WMS_INBD_TRX_PK          AS  WMS_INBD_TRX_PK
                      , WIT.EZINTIME                 AS  EZINTIME
                      , WIT.EZUPTIME                 AS  EZUPTIME
                      , WIT.WH_CD                    AS  WH_CD
                      , WIT.WMS_TASK_CD              AS  WMS_TASK_CD
                      , WIT.PROC_STS_CD              AS  PROC_STS_CD
                      , WIT.ERR_MSG_CD               AS  ERR_MSG_CD
                      , WIT.WMS_MDSE_CD              AS  WMS_MDSE_CD
                      , WIT.SER_NUM                  AS  SER_NUM
                      , WIT.WMS_ORD_QTY              AS  WMS_ORD_QTY
                      , WIT.INBD_ORD_NUM             AS  INBD_ORD_NUM
                      , WIT.OTBD_ORD_NUM             AS  OTBD_ORD_NUM
                      , WIT.EZUPTIMEZONE             AS  EZUPTIMEZONE
                  FROM
                        WMS_INBD_TRX                 WIT
                 WHERE
                        WIT.GLBL_CMPY_CD           = #glblCmpyCd#
                   <isNotNull property="whCd">
                   AND  WIT.WH_CD                  = #whCd#
                   </isNotNull>
                   <isNotNull property="wmsTaskCd">
                   AND  WIT.WMS_TASK_CD            = #wmsTaskCd#
                   </isNotNull>
                   <isNotNull property="procStsCd">
                   AND  WIT.PROC_STS_CD            = #procStsCd#
                   </isNotNull>
                   <isNotNull property="wmsInbdTrxPk">
                   AND  WIT.WMS_INBD_TRX_PK        = #wmsInbdTrxPk#
                   </isNotNull>
                   <isNotNull property="wmsMdseCd">
                   AND  WIT.WMS_MDSE_CD            = #wmsMdseCd#
                   </isNotNull>
                   AND  WIT.EZCANCELFLAG           = '0'
              ORDER BY
                        WIT.WMS_INBD_TRX_PK DESC
               )
                WIT
         WHERE
                ROWNUM &lt;= #rownum#

    </statement>

    <resultMap id="result.NLGL0060_ASMsg" class="NLGL0060_ASMsg">
        <result property="wmsInbdTrxPk_A1"           column="WMS_INBD_TRX_PK"              javaType="EZDSStringItem"/>
        <result property="ezInTime_A1"               column="EZINTIME"                     javaType="EZDSStringItem"/>
        <result property="ezUpTime_A1"               column="EZUPTIME"                     javaType="EZDSStringItem"/>
        <result property="whCd_A1"                   column="WH_CD"                        javaType="EZDSStringItem"/>
        <result property="wmsTaskCd_A1"              column="WMS_TASK_CD"                  javaType="EZDSStringItem"/>
        <result property="procStsCd_A1"              column="PROC_STS_CD"                  javaType="EZDSStringItem"/>
        <result property="errMsgCd_A1"               column="ERR_MSG_CD"                   javaType="EZDSStringItem"/>
        <result property="wmsMdseCd_A1"              column="WMS_MDSE_CD"                  javaType="EZDSStringItem"/>
        <result property="serNum_A1"                 column="SER_NUM"                      javaType="EZDSStringItem"/>
        <result property="wmsOrdQty_A1"              column="WMS_ORD_QTY"                  javaType="EZDSStringItem"/>
        <result property="inbdOrdNum_A1"             column="INBD_ORD_NUM"                 javaType="EZDSStringItem"/>
        <result property="otbdOrdNum_A1"             column="OTBD_ORD_NUM"                 javaType="EZDSStringItem"/>
        <result property="ezUpTimeZone_A1"           column="EZUPTIMEZONE"                 javaType="EZDSStringItem"/>
        <result property="procStsCd_BK"              column="PROC_STS_CD"                  javaType="EZDSStringItem"/>
    </resultMap>

</sqlMap>