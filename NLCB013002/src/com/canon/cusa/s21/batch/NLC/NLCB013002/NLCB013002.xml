<sqlMap namespace="NLCB013002">
    <statement id="getInterfaceWE" parameterClass="Map">
    SELECT
         W.GLBL_CMPY_CD
        ,W.MDSE_CD
        ,W.LOCAL_MDSE_CD
        ,W.VND_GLBL_CMPY_CD
        ,SUM(W.WRK_DB_QTY_01) WRK_DB_QTY_01
     FROM
         STK_IN_RSLT_WRK W
    WHERE
              W.GLBL_CMPY_CD  =  #glblCmpyCd#
         AND  W.WRK_DB_QTY_01 &lt;&gt;  0
         AND  W.EZCANCELFLAG  =  '0'
    GROUP BY
         W.GLBL_CMPY_CD
        ,W.MDSE_CD
        ,W.LOCAL_MDSE_CD
        ,W.VND_GLBL_CMPY_CD
    ORDER BY
         W.GLBL_CMPY_CD
        ,W.MDSE_CD
        ,W.LOCAL_MDSE_CD
        ,W.VND_GLBL_CMPY_CD
    </statement>

