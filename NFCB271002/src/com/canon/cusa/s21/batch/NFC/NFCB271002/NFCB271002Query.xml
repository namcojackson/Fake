<?xml version="1.0" encoding="UTF-8"?>
<sqlMap namespace="NFCB271002Query">
    <typeAlias alias="AR_DLY_OP_INFOTMsg" type="business.db.AR_DLY_OP_INFOTMsg"/>
    <statement id="getThruArTrxBalPk" parameterClass="Map" resultMap="result.AR_DLY_OP_INFOTMsg">
     SELECT
               MAX(THRU_AR_TRX_BAL_PK)   THRU_AR_TRX_BAL_PK
     FROM
               AR_DLY_OP_INFO
     WHERE
                    GLBL_CMPY_CD  =  #glblCmpyCd#
               AND  EZCANCELFLAG  =  '0'
    </statement>
    <!-- AR_DLY_OP_INFOTMsg Mapping -->
    <resultMap id="result.AR_DLY_OP_INFOTMsg" class="AR_DLY_OP_INFOTMsg">
        <result property="thruArTrxBalPk" column="THRU_AR_TRX_BAL_PK" javaType="EZDTBigDecimalItem"/>
    </resultMap>

    <typeAlias alias="AR_TRX_BALTMsg" type="business.db.AR_TRX_BALTMsg"/>
    <statement id="getArTrxBalPk" parameterClass="Map" resultMap="result.AR_TRX_BALTMsg">
     SELECT
               MAX(AR_TRX_BAL_PK)   AR_TRX_BAL_PK
     FROM
               AR_TRX_BAL
     WHERE
                    GLBL_CMPY_CD  =  #glblCmpyCd#
               AND  EZCANCELFLAG  =  '0'
    </statement>
    <!-- AR_TRX_BALTMsg Mapping -->
    <resultMap id="result.AR_TRX_BALTMsg" class="AR_TRX_BALTMsg">
        <result property="arTrxBalPk" column="AR_TRX_BAL_PK" javaType="EZDTBigDecimalItem"/>
    </resultMap>
</sqlMap>
