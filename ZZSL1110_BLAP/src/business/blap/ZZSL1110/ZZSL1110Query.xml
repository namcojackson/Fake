<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="ZZSL1110Query">
	
	<typeAlias alias="ZZSL1110_ACMsg" type="business.blap.ZZSL1110.ZZSL1110_ACMsg"/>
    <typeAlias alias="ZZSL1110_ACMsgArray" type="business.blap.ZZSL1110.ZZSL1110_ACMsgArray"/>
	
	<statement id="getCompanyListOnCriteria" resultMap="result.ZZSL1110_ACMsg" parameterClass="Map">
		SELECT 
			GLBL_CMPY_CD, GLBL_CMPY_NM
		FROM 
			( SELECT ROWNUM RNUM, A.* FROM
			( SELECT EZCANCELFLAG, GLBL_CMPY_CD, GLBL_CMPY_NM FROM GLBL_CMPY ORDER BY GLBL_CMPY_CD) A 
		WHERE
        	EZCANCELFLAG = 0
        	
        <isNotNull property="compCode">
			AND UPPER(GLBL_CMPY_CD) LIKE #compCode#
		</isNotNull>
		<isNotNull property="compName">
			AND UPPER(GLBL_CMPY_NM) LIKE #compName#
		</isNotNull>

      	<isNotNull property="maxRow">	
    	    AND ROWNUM &lt;= #maxRow#
	   	 </isNotNull> 
	   	  )
	   	WHERE
        <isNotNull property="minRow">	
	       RNUM &gt;= #minRow#
        </isNotNull>
        
	</statement>
	
	
	<resultMap id="result.ZZSL1110_ACMsg" class="ZZSL1110_ACMsg">
		<result property="glblCmpyCd_A" column="GLBL_CMPY_CD" javaType="StringItemEZD"/>
		<result property="glblCmpyNm_A" column="GLBL_CMPY_NM" javaType="StringItemEZD"/>
	</resultMap>
	
</sqlMap>