    <statement id="getDsContrNumByCpoOrdNum" parameterClass="Map" resultClass="String">
        SELECT
            CD.DS_CONTR_NUM
        FROM
            CPO_DTL CD
        WHERE
            CD.GLBL_CMPY_CD = #glblCmpyCd#
            AND CD.CPO_ORD_NUM = #cpoOrdNum#
            AND CD.DS_CONTR_NUM IS NOT NULL
            AND CD.EZCANCELFLAG = '0'
        GROUP BY
            CD.DS_CONTR_NUM
    </statement>
