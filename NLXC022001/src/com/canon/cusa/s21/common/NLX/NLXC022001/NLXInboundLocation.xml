<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="NLXInboundLocation">

    <!-- ==== Type Aliases ==== -->

    <!-- ==== Result Mapping ==== -->

    <statement id="findWhListForPullDown" parameterClass="Map">
     SELECT
           W.WH_CD    WH_CD
          ,W.LOC_NM   LOC_NM
     FROM
           WH  W
     WHERE
               W.GLBL_CMPY_CD  =  #glblCmpyCd#
        <isNotNull property="whCd">
           AND W.WH_CD
            <iterate property="whCd" var="elem[]" open=" IN (" close=")" conjunction=",">
           #elem[]#
            </iterate>
        </isNotNull>
        <isNotNull property="rgtnStsCd">
           AND W.RGTN_STS_CD
            <iterate property="rgtnStsCd" var="elem[]" open=" IN (" close=")" conjunction=",">
           #elem[]#
            </iterate>
        </isNotNull>

     ORDER BY
               W.WH_CD  ASC
    </statement>
    
    <statement id="findVndListForPullDown" parameterClass="Map">
     SELECT
           VR.VND_CD   VND_CD
          ,VR.LOC_NM   LOC_NM
     FROM
           VND          VR
          ,VND_TP_RELN  VTR
     WHERE
               VR.GLBL_CMPY_CD   =  #glblCmpyCd#
          AND  VR.GLBL_CMPY_CD   =  VTR.GLBL_CMPY_CD
          AND  VR.VND_CD         =  VTR.VND_CD
        <isNotNull property="vndCd">
          AND  VR.VND_CD
          <iterate property="vndCd" var="elem[]" open=" IN (" close=")" conjunction=",">
            #elem[]#
          </iterate>
        </isNotNull>
        <isNotNull property="rgtnStsCd">
          AND  VR.RGTN_STS_CD
          <iterate property="rgtnStsCd" var="elem[]" open=" IN (" close=")" conjunction=",">
            #elem[]#
          </iterate>
        </isNotNull>
          AND  VTR.VND_TP_CD
        <iterate property="vndTpCd" var="elem[]" open=" IN (" close=")" conjunction=",">
          #elem[]#
        </iterate>
          AND  VR.EZCANCELFLAG   =  '0'
          AND  VTR.EZCANCELFLAG  =  '0'
     ORDER BY
               VR.VND_CD  ASC
    </statement>
    
</sqlMap>
