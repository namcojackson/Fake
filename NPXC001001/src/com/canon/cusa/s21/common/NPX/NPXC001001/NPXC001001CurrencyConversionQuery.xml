<sqlMap namespace="NPXC001001CurrencyConversionQuery">
	<typeAlias alias="ACCT_DLY_ACTL_EXCH_RATESTMsg_alias" type="business.db.ACCT_DLY_ACTL_EXCH_RATESTMsg"/>
    
	<statement id="getActlExchRate" parameterClass="Map" resultMap="result.ACCT_DLY_ACTL_EXCH_RATESTMsg">
     SELECT
               A.ACTL_EXCH_RATE
     FROM
               ACCT_DLY_ACTL_EXCH_RATES A
     WHERE
                   A.GLBL_CMPY_CD  			    =  #glblCmpyCd#
               AND A.CCY_CD        			    =  #ccyCd#
               AND A.ACTL_EXCH_RATE_ENT_DT	&lt;=  #actlExchRateEndDt#
               AND A.EZCANCELFLAG  			    =  '0'
               AND ROWNUM                       = 1
     ORDER BY
                   A.ACTL_EXCH_RATE_ENT_DT  DESC
	</statement>
	<!-- ACCT_DLY_ACTL_EXCH_RATESTMsg Mapping -->
    <resultMap id="result.ACCT_DLY_ACTL_EXCH_RATESTMsg" class="ACCT_DLY_ACTL_EXCH_RATESTMsg_alias">
        <result property="actlExchRate" column="ACTL_EXCH_RATE" javaType="EZDTBigDecimalItem"/>
    </resultMap>
</sqlMap>
