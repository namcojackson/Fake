<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NMAL6350Query">

	<typeAlias alias="NMAL6350CMsg"		type="business.blap.NMAL6350.NMAL6350CMsg"/>
	<typeAlias alias="NMAL6350_ASMsg"	type="business.blap.NMAL6350.NMAL6350_ASMsg"/>

	<resultMap id="result.getNMAL6350"    class="NMAL6350_ASMsg">
		<result property="glblCmpyCd_A1"		column="GLBL_CMPY_CD"			javaType="EZDCStringItem"/>
		<result property="pkgUomSortNum_A1"		column="PKG_UOM_SORT_NUM"		javaType="EZDCBigDecimalItem"/>
		<result property="pkgUomCd_A1"			column="PKG_UOM_CD"				javaType="EZDCStringItem"/>
		<result property="pkgUomNm_A1"			column="PKG_UOM_NM"				javaType="EZDCStringItem"/>
		<result property="pkgUomDescTxt_A1"		column="PKG_UOM_DESC_TXT"		javaType="EZDCStringItem"/>
		<result property="pkgUomStdFlg_A1"		column="PKG_UOM_STD_FLG"		javaType="EZDCStringItem"/>
		<result property="pkgUomMndFlg_A1"		column="PKG_UOM_MND_FLG"		javaType="EZDCStringItem"/>
		<result property="pkgUomPkgLvlNum_A1"	column="PKG_UOM_PKG_LVL_NUM"	javaType="EZDCStringItem"/>
		<result property="pkgUomClsCd_A1"		column="PKG_UOM_CLS_CD"		javaType="EZDCStringItem"/>  <!-- 2018/09/04 QC#28019 Add -->
		<result property="ezUpTime_A1"			column="EZUPTIME"			javaType="EZDCStringItem"/>
		<result property="ezUpTimeZone_A1"		column="EZUPTIMEZONE"		javaType="EZDCStringItem"/>
	</resultMap>
	
    <statement id="getCodeTableData" parameterClass="NMAL6350CMsg" resultMap="result.getNMAL6350">
        SELECT 
			 GLBL_CMPY_CD
			,PKG_UOM_CD
			,PKG_UOM_NM
			,PKG_UOM_SORT_NUM
			,PKG_UOM_DESC_TXT
			,PKG_UOM_STD_FLG
			,PKG_UOM_MND_FLG
			,PKG_UOM_PKG_LVL_NUM
			,PKG_UOM_CLS_CD  <!-- 2018/09/04 QC#28019 Add -->
			,NVL(EZUPTIME, '*') EZUPTIME
			,EZUPTIMEZONE
        FROM 
			PKG_UOM
        WHERE
			   GLBL_CMPY_CD = #glblCmpyCd#
          AND  EZCANCELFLAG = '0'
        ORDER BY
            PKG_UOM_SORT_NUM
    </statement>


</sqlMap>
