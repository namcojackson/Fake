<sqlMap namespace="NFXC304001Query">
	<typeAlias alias="AR_RCPT_RCV_WRKTMsg_alias" type="business.db.AR_RCPT_RCV_WRKTMsg"/>
	<statement id="getRcvFuncId" parameterClass="Map" resultMap="result.AR_RCPT_RCV_WRKTMsg">
     SELECT
               A.RCV_FUNC_ID
              ,A.EZINTIME
              ,A.ACCT_MA_CD
     FROM
               AR_RCPT_RCV_WRK A
     WHERE
                   A.GLBL_CMPY_CD  =  #glblCmpyCd#
               AND A.RCV_SQ_PK     =  #rcvSqPk#
               AND A.RCV_HDR_NUM   =  #rcvHdrNum#
               AND A.RCV_DTL_NUM   =  '0001'
               AND A.EZCANCELFLAG  =  '0'
	</statement>
	<!-- AR_RCPT_RCV_WRKTMsg Mapping -->
	<resultMap id="result.AR_RCPT_RCV_WRKTMsg" class="AR_RCPT_RCV_WRKTMsg_alias">
	    <result property="rcvFuncId" column="RCV_FUNC_ID" javaType="EZDTStringItem"/>
	    <result property="ezInTime" column="EZINTIME" javaType="EZDTStringItem"/>
	    <result property="acctMaCd" column="ACCT_MA_CD" javaType="EZDTStringItem"/>
	</resultMap>


	<typeAlias alias="AR_RCPT_IN_PROC_WRKTMsg_alias" type="business.db.AR_RCPT_IN_PROC_WRKTMsg"/>
	<statement id="getEdiSendBankCd" parameterClass="Map" resultMap="result.AR_RCPT_IN_PROC_WRKTMsg">
     SELECT
               A.AR_EDI_SEND_BANK_CD
     FROM
               AR_RCPT_IN_PROC_WRK A
     WHERE
                   A.GLBL_CMPY_CD        =  #glblCmpyCd#
               AND A.RCPT_IN_PROC_SQ_PK  =  #rcptInProcSqPk#
               AND A.RCV_HDR_NUM         =  #rcvHdrNum#
               AND A.RCV_DTL_NUM         =  '0001'
               AND A.EZCANCELFLAG        =  '0'
	</statement>
	<!-- AR_RCPT_IN_PROC_WRKTMsg Mapping -->
    <resultMap id="result.AR_RCPT_IN_PROC_WRKTMsg" class="AR_RCPT_IN_PROC_WRKTMsg_alias">
        <result property="arEdiSendBankCd" column="AR_EDI_SEND_BANK_CD" javaType="EZDTStringItem"/>
    </resultMap>
</sqlMap>
