<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sqlMap namespace="NWAL0300Query">


	<typeAlias alias="NWAL0300CMsg" type="business.blap.NWAL0300.NWAL0300CMsg"/>

    <statement id="getHld" parameterClass="NWAL0300CMsg" resultMap="result.getHld">
       SELECT
              H.CPO_ORD_NUM
            , H.CPO_DTL_LINE_NUM
            , H.CPO_DTL_LINE_SUB_NUM
            , H.HLD_DT
            , H.HLD_DTL_TXT
         FROM
              HLD H
        WHERE
			  H.EZCANCELFLAG = '0'
          AND H.GLBL_CMPY_CD = #glblCmpyCd#
          AND H.HLD_PK = #hldPk#
    </statement>

	<resultMap id="result.getHld" class="NWAL0300CMsg">
		<result property="cpoOrdNum" 		column="CPO_ORD_NUM" 			javaType="EZDCStringItem"/>
		<result property="cpoDtlLineNum" 	column="CPO_DTL_LINE_NUM" 		javaType="EZDCStringItem"/>
		<result property="cpoDtlLineSubNum" column="CPO_DTL_LINE_SUB_NUM" 	javaType="EZDCStringItem"/>
		<result property="hldDt" 			column="HLD_DT" 				javaType="EZDCDateItem"/>
		<result property="hldDtlTxt" 		column="HLD_DTL_TXT" 			javaType="EZDCStringItem"/>
	</resultMap>

    <statement id="getCPOOrderInfo" parameterClass="NWAL0300CMsg" resultMap="result.getCPOOrderInfo">
       SELECT
              C.CPO_ORD_TP_CD
            , C.ADD_DROP_SHIP_FLG
            , C.ADD_SHIP_TO_LOC_NM
            , C.ADD_SHIP_TO_ADDL_LOC_NM
         FROM
              CPO C
        WHERE
			  C.EZCANCELFLAG = '0'
          AND C.GLBL_CMPY_CD = #glblCmpyCd#
          AND C.CPO_ORD_NUM = #cpoOrdNum#
    </statement>

	<resultMap id="result.getCPOOrderInfo" class="NWAL0300CMsg">
		<result property="cpoOrdTpCd" 			column="CPO_ORD_TP_CD" 				javaType="EZDCStringItem"/>
		<result property="dropShipFlg" 		column="ADD_DROP_SHIP_FLG" 			javaType="EZDCStringItem"/>
		<result property="shipToLocNm" 		column="ADD_SHIP_TO_LOC_NM" 		javaType="EZDCStringItem"/>
		<result property="shipToAddlLocNm" 	column="ADD_SHIP_TO_ADDL_LOC_NM" 	javaType="EZDCStringItem"/>
	</resultMap>


    <statement id="getCPODtlOrderInfo" parameterClass="NWAL0300CMsg" resultMap="result.getCPODtlOrderInfo">
       SELECT
              CD.CPO_ORD_TP_CD
            , CD.DROP_SHIP_FLG
            , CD.SHIP_TO_LOC_NM
            , CD.SHIP_TO_ADDL_LOC_NM
         FROM
              CPO_DTL CD
        WHERE
			  CD.EZCANCELFLAG = '0'
          AND CD.GLBL_CMPY_CD = #glblCmpyCd#
          AND CD.CPO_ORD_NUM = #cpoOrdNum#
          AND CD.CPO_DTL_LINE_NUM = #cpoDtlLineNum#
          AND CD.CPO_DTL_LINE_SUB_NUM = #cpoDtlLineSubNum#
    </statement>

	<resultMap id="result.getCPODtlOrderInfo" class="NWAL0300CMsg">
		<result property="cpoOrdTpCd" 			column="CPO_ORD_TP_CD" 			javaType="EZDCStringItem"/>
		<result property="dropShipFlg" 		column="DROP_SHIP_FLG" 			javaType="EZDCStringItem"/>
		<result property="shipToLocNm" 		column="SHIP_TO_LOC_NM" 		javaType="EZDCStringItem"/>
		<result property="shipToAddlLocNm" 	column="SHIP_TO_ADDL_LOC_NM" 	javaType="EZDCStringItem"/>
	</resultMap>


    <statement id="getCPOOrdTp" parameterClass="NWAL0300CMsg" resultMap="result.getCPOOrdTp">
       SELECT
              COT.CPO_ORD_TP_NM
         FROM
              CPO_ORD_TP COT
        WHERE
			  COT.EZCANCELFLAG = '0'
          AND COT.GLBL_CMPY_CD = #glblCmpyCd#
          AND COT.CPO_ORD_TP_CD = #cpoOrdTpCd#
    </statement>

	<resultMap id="result.getCPOOrdTp" class="NWAL0300CMsg">
		<result property="cpoOrdTpNm" 	column="CPO_ORD_TP_NM" javaType="EZDCStringItem"/>
	</resultMap>

    <statement id="getSellToCust" parameterClass="NWAL0300CMsg" resultMap="result.getSellToCust">
       SELECT
              SE.LOC_NM
            , SE.ADDL_LOC_NM
         FROM
              SELL_TO_CUST SE
        WHERE
			  SE.EZCANCELFLAG = '0'
          AND SE.GLBL_CMPY_CD = #glblCmpyCd#
          AND SE.SELL_TO_CUST_CD = #sellToCustCd#
    </statement>

	<resultMap id="result.getSellToCust" class="NWAL0300CMsg">
		<result property="locNm_H1" 		column="LOC_NM" 		javaType="EZDCStringItem"/>
		<result property="addlLocNm_H1"		column="ADDL_LOC_NM" 	javaType="EZDCStringItem"/>
	</resultMap>

    <statement id="getBillToCust" parameterClass="NWAL0300CMsg" resultMap="result.getBillToCust">
       SELECT
              BT.LOC_NM
            , BT.ADDL_LOC_NM
         FROM
              BILL_TO_CUST BT
        WHERE
			  BT.EZCANCELFLAG = '0'
          AND BT.GLBL_CMPY_CD = #glblCmpyCd#
          AND BT.BILL_TO_CUST_CD = #billToCustCd#
    </statement>

	<resultMap id="result.getBillToCust" class="NWAL0300CMsg">
		<result property="locNm_H2" 		column="LOC_NM" 		javaType="EZDCStringItem"/>
		<result property="addlLocNm_H2"		column="ADDL_LOC_NM" 	javaType="EZDCStringItem"/>
	</resultMap>

    <statement id="getShipToCust" parameterClass="NWAL0300CMsg" resultMap="result.getShipToCust">
       SELECT
              SH.LOC_NM
            , SH.ADDL_LOC_NM
         FROM
              SHIP_TO_CUST SH
        WHERE
			  SH.EZCANCELFLAG = '0'
          AND SH.GLBL_CMPY_CD = #glblCmpyCd#
          AND SH.SHIP_TO_CUST_CD = #shipToCustCd#
    </statement>

	<resultMap id="result.getShipToCust" class="NWAL0300CMsg">
		<result property="locNm_H3" 		column="LOC_NM" 		javaType="EZDCStringItem"/>
		<result property="addlLocNm_H3"		column="ADDL_LOC_NM" 	javaType="EZDCStringItem"/>
	</resultMap>

    <statement id="getMdse" parameterClass="Map" resultMap="result.getMdse">
       SELECT
              M.MDSE_NM
         FROM
              MDSE M
        WHERE
			  M.EZCANCELFLAG = '0'
          AND M.GLBL_CMPY_CD = #glblCmpyCd#
          AND M.MDSE_CD = #mdseCd#
    </statement>

	<resultMap id="result.getMdse" class="NWAL0300CMsg">
		<result property="mdseNm" 	column="MDSE_NM" 	javaType="EZDSStringItem"/>
	</resultMap>

</sqlMap>
