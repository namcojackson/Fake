<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<sqlMap namespace="NLGL0040Query">
    <typeAlias alias="NLGL0040_ASMsg"  type="business.blap.NLGL0040.NLGL0040_ASMsg"/>
    <typeAlias alias="NLGL0040SMsg"    type="business.blap.NLGL0040.NLGL0040SMsg"/>

    <statement id="getWHPullDownList" parameterClass="Map" resultClass="Map">
    SELECT
           WH.WH_CD
          ,WH.WMS_DESC_NM
      FROM
           WMS_WH WH
     WHERE
           WH.GLBL_CMPY_CD         = #glblCmpyCd#
       AND WH.MW_REPL_TRGT_GRP_CD IS NOT NULL
       AND WH.EZCANCELFLAG         = '0'
     ORDER BY
           WH.WH_CD
    </statement>

    <statement id="getSHIPVIAList" parameterClass="Map" resultMap="result.NLGL0040_ASMsg">
    SELECT
           SV.WMS_SHIP_VIA_TP_CD WMS_SHIP_VIA_TP_CD
          ,SV.RTE_GUIDE_NUM      RTE_GUIDE_NUM
          ,SV.WMS_DESC_TXT       WMS_DESC_TXT
          ,SV.MD_BREAK_TP_CD     MD_BREAK_TP_CD
          ,SV.PCL_CARR_CD        PCL_CARR_CD
          ,SV.LTL_CARR_CD        LTL_CARR_CD
          ,SV.TL_CARR_CD         TL_CARR_CD
          ,SV.PCL_MAX_CAP_NUM    PCL_MAX_CAP_NUM
          ,SV.LTL_MAX_CAP_NUM    LTL_MAX_CAP_NUM
          ,SV.TL_MAX_CAP_NUM     TL_MAX_CAP_NUM
          ,SV.PCL_PRTY_CD        PCL_PRTY_CD
          ,SV.LTL_PRTY_CD        LTL_PRTY_CD
          ,SV.TL_PRTY_CD         TL_PRTY_CD
          ,SV.EZUPUSERID         EZUPUSERID
          ,SV.EZUPTIME           EZUPTIME
          ,SV.EZUPTIMEZONE       EZUPTIMEZONE
      FROM
           WMS_SHIP_VIA_RTE_MAP SV
     WHERE
           SV.GLBL_CMPY_CD       =  #glblCmpyCd#
       AND SV.WH_CD              =  #whCd#
       AND SV.EZCANCELFLAG       =  '0'
       AND ROWNUM            &lt;=  #rownum# 
     ORDER BY
           SV.WH_CD ASC
          ,SV.WMS_SHIP_VIA_TP_CD ASC
    </statement>

    <resultMap id="result.NLGL0040_ASMsg" class="NLGL0040_ASMsg">
        <result property="wmsShipViaTpCd_D1" column="WMS_SHIP_VIA_TP_CD" javaType="EZDSStringItem"/>
        <result property="wmsDescTxt_D1"     column="WMS_DESC_TXT"       javaType="EZDSStringItem"/>
        <result property="mdBreakTpCd_D1"    column="MD_BREAK_TP_CD"     javaType="EZDSStringItem"/>
        <result property="rteGuideNum_D1"    column="RTE_GUIDE_NUM"      javaType="EZDSStringItem"/>
        <result property="pclCarrCd_D1"      column="PCL_CARR_CD"        javaType="EZDSStringItem"/>
        <result property="pclMaxCapNum_D1"   column="PCL_MAX_CAP_NUM"    javaType="EZDSBigDecimalItem"/>
        <result property="pclPrtyCd_D1"      column="PCL_PRTY_CD"        javaType="EZDSStringItem"/>
        <result property="ltlCarrCd_D1"      column="LTL_CARR_CD"        javaType="EZDSStringItem"/>
        <result property="ltlMaxCapNum_D1"   column="LTL_MAX_CAP_NUM"    javaType="EZDSBigDecimalItem"/>
        <result property="ltlPrtyCd_D1"      column="LTL_PRTY_CD"        javaType="EZDSStringItem"/>
        <result property="tlCarrCd_D1"       column="TL_CARR_CD"         javaType="EZDSStringItem"/>
        <result property="tlMaxCapNum_D1"    column="TL_MAX_CAP_NUM"     javaType="EZDSBigDecimalItem"/>
        <result property="tlPrtyCd_D1"       column="TL_PRTY_CD"         javaType="EZDSStringItem"/>
        <result property="ezUpUserID_D1"     column="EZUPUSERID"         javaType="EZDSStringItem"/>
        <result property="ezUpTime_D1"       column="EZUPTIME"           javaType="EZDSStringItem"/>
        <result property="ezUpTimeZone_D1"   column="EZUPTIMEZONE"       javaType="EZDSStringItem"/>
    </resultMap>

    <statement id="getSHIPVIAEdit" parameterClass="Map" resultMap="result.NLGL0040SMsg">
    SELECT
           SV.WMS_SHIP_VIA_TP_CD WMS_SHIP_VIA_TP_CD
          ,SV.RTE_GUIDE_NUM      RTE_GUIDE_NUM
          ,SV.WMS_DESC_TXT       WMS_DESC_TXT
          ,SV.MD_BREAK_TP_CD     MD_BREAK_TP_CD
          ,SV.PCL_CARR_CD        PCL_CARR_CD
          ,SV.LTL_CARR_CD        LTL_CARR_CD
          ,SV.TL_CARR_CD         TL_CARR_CD
          ,SV.PCL_MAX_CAP_NUM    PCL_MAX_CAP_NUM
          ,SV.LTL_MAX_CAP_NUM    LTL_MAX_CAP_NUM
          ,SV.TL_MAX_CAP_NUM     TL_MAX_CAP_NUM
          ,SV.PCL_PRTY_CD        PCL_PRTY_CD
          ,SV.LTL_PRTY_CD        LTL_PRTY_CD
          ,SV.TL_PRTY_CD         TL_PRTY_CD
          ,SV.EZUPUSERID         EZUPUSERID
          ,SV.EZUPTIME           EZUPTIME
          ,SV.EZUPTIMEZONE       EZUPTIMEZONE
      FROM
           WMS_SHIP_VIA_RTE_MAP SV
     WHERE
           SV.GLBL_CMPY_CD       =  #glblCmpyCd#
       AND SV.WH_CD              =  #whCd#
       AND SV.WMS_SHIP_VIA_TP_CD =  #wmsShipViaTpCd#
       AND SV.EZCANCELFLAG       =  '0'
     ORDER BY
           SV.WH_CD ASC
          ,SV.WMS_SHIP_VIA_TP_CD ASC
    </statement>

    <resultMap id="result.NLGL0040SMsg" class="NLGL0040SMsg">
        <result property="wmsShipViaTpCd_D2" column="WMS_SHIP_VIA_TP_CD" javaType="EZDSStringItem"/>
        <result property="wmsDescTxt_D2"     column="WMS_DESC_TXT"       javaType="EZDSStringItem"/>
        <result property="mdBreakTpCd_P2"    column="MD_BREAK_TP_CD"     javaType="EZDSStringItem"/>
        <result property="rteGuideNum_D2"    column="RTE_GUIDE_NUM"      javaType="EZDSStringItem"/>
        <result property="pclCarrCd_D2"      column="PCL_CARR_CD"        javaType="EZDSStringItem"/>
        <result property="pclMaxCapNum_D2"   column="PCL_MAX_CAP_NUM"    javaType="EZDSBigDecimalItem"/>
        <result property="pclPrtyCd_D2"      column="PCL_PRTY_CD"        javaType="EZDSStringItem"/>
        <result property="ltlCarrCd_D2"      column="LTL_CARR_CD"        javaType="EZDSStringItem"/>
        <result property="ltlMaxCapNum_D2"   column="LTL_MAX_CAP_NUM"    javaType="EZDSBigDecimalItem"/>
        <result property="ltlPrtyCd_D2"      column="LTL_PRTY_CD"        javaType="EZDSStringItem"/>
        <result property="tlCarrCd_D2"       column="TL_CARR_CD"         javaType="EZDSStringItem"/>
        <result property="tlMaxCapNum_D2"    column="TL_MAX_CAP_NUM"     javaType="EZDSBigDecimalItem"/>
        <result property="tlPrtyCd_D2"       column="TL_PRTY_CD"         javaType="EZDSStringItem"/>
        <result property="ezUpUserID_D2"     column="EZUPUSERID"         javaType="EZDSStringItem"/>
        <result property="ezUpTime_D2"       column="EZUPTIME"           javaType="EZDSStringItem"/>
        <result property="ezUpTimeZone_D2"   column="EZUPTIMEZONE"       javaType="EZDSStringItem"/>
    </resultMap>
</sqlMap>
